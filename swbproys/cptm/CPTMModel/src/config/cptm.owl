<?xml version="1.0"?>
<rdf:RDF
    xmlns:swbxf="http://www.semanticwebbuilder.org/swb4/xforms/ontology#"
    xmlns:cptm="http://www.semanticwebbuilder.org/swb4/cptm#"
    xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
    xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
    xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
    xmlns:owl="http://www.w3.org/2002/07/owl#"
    xmlns:dp_="http://www.semanticwebbuilder.org/swb4/community#dp_@"
    xmlns:swb="http://www.semanticwebbuilder.org/swb4/ontology#"
  xml:base="http://www.semanticwebbuilder.org/swb4/cptm">
  <owl:Ontology rdf:about="">
    <owl:imports rdf:resource="http://www.semanticwebbuilder.org/swb4/community"/>
    <owl:versionInfo rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >Created with TopBraid Composer</owl:versionInfo>
  </owl:Ontology>
  <owl:Class rdf:ID="Festivity">
    <swb:displayObject rdf:resource="http://www.semanticwebbuilder.org/swb4/community#do_WebPage"/>
    <rdfs:label xml:lang="en">Festivity</rdfs:label>
    <rdfs:label xml:lang="es">Festividad</rdfs:label>
    <rdfs:subClassOf rdf:resource="http://www.semanticwebbuilder.org/swb4/ontology#WebPage"/>
    <rdfs:comment>Clase que se encarga de administrar Festividades. Ej. "Día de Muertos"</rdfs:comment>
    <rdf:type rdf:resource="http://www.semanticwebbuilder.org/swb4/ontology#Class"/>
  </owl:Class>
  <owl:Class rdf:ID="TravelRoute">
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:allValuesFrom>
          <swb:Class rdf:ID="TravelRouteType">
            <rdfs:subClassOf>
              <owl:Restriction>
                <owl:allValuesFrom rdf:resource="#TravelRoute"/>
                <owl:onProperty rdf:resource="http://www.semanticwebbuilder.org/swb4/ontology#hasWebPageChild"/>
              </owl:Restriction>
            </rdfs:subClassOf>
            <swb:displayObject rdf:resource="http://www.semanticwebbuilder.org/swb4/community#do_WebPage"/>
            <rdfs:label xml:lang="es">Tipo de Ruta de Viaje</rdfs:label>
            <rdfs:label xml:lang="en">Travel Route Type</rdfs:label>
            <rdfs:subClassOf rdf:resource="http://www.semanticwebbuilder.org/swb4/ontology#WebPage"/>
            <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
          </swb:Class>
        </owl:allValuesFrom>
        <owl:onProperty rdf:resource="http://www.semanticwebbuilder.org/swb4/ontology#webPageParent"/>
      </owl:Restriction>
    </rdfs:subClassOf>
    <swb:displayObject rdf:resource="http://www.semanticwebbuilder.org/swb4/community#do_WebPage"/>
    <rdfs:subClassOf rdf:resource="http://www.semanticwebbuilder.org/swb4/ontology#WebPage"/>
    <rdfs:comment>Clase que se encarga de manejar las rutas de viaje</rdfs:comment>
    <rdfs:label xml:lang="en">Travel Route</rdfs:label>
    <rdfs:label xml:lang="es">Ruta de Viaje</rdfs:label>
    <rdf:type rdf:resource="http://www.semanticwebbuilder.org/swb4/ontology#Class"/>
  </owl:Class>
  <owl:Class rdf:ID="ExperienceRef">
    <rdfs:label xml:lang="en">Experience Destination</rdfs:label>
    <rdfs:label xml:lang="es">Experiencia en Destino</rdfs:label>
    <rdfs:comment>Experiencia en un específico "Destino", si le quisiera agregar un Directorio de empresas aqui, tendría que tener una relación además de con el Destino, con el objeto  SPType</rdfs:comment>
    <swb:displayObject rdf:resource="http://www.semanticwebbuilder.org/swb4/community#do_WebPage"/>
    <rdfs:subClassOf rdf:resource="http://www.semanticwebbuilder.org/swb4/ontology#SWBClass"/>
    <rdf:type rdf:resource="http://www.semanticwebbuilder.org/swb4/ontology#Class"/>
  </owl:Class>
  <owl:Class rdf:ID="EventCategory">
    <rdfs:subClassOf rdf:resource="http://www.semanticwebbuilder.org/swb4/ontology#SWBClass"/>
    <rdfs:comment>Categorías para los tipos de Eventos, ej. "Turisticos", Industria de Negocio"</rdfs:comment>
    <swb:displayObject rdf:resource="http://www.semanticwebbuilder.org/swb4/community#do_WebPage"/>
    <rdfs:subClassOf rdf:resource="http://www.semanticwebbuilder.org/swb4/ontology#Descriptiveable"/>
    <rdfs:subClassOf rdf:resource="http://www.semanticwebbuilder.org/swb4/ontology#Traceable"/>
    <swb:autogenId rdf:datatype="http://www.w3.org/2001/XMLSchema#boolean"
    >false</swb:autogenId>
    <rdf:type rdf:resource="http://www.semanticwebbuilder.org/swb4/ontology#Class"/>
  </owl:Class>
  <owl:Class rdf:ID="CPTMWebSite">
    <swb:hasHerarquicalNode rdf:resource="http://www.semanticwebbuilder.org/swb4/xforms/ontology#hn_Rules"/>
    <rdfs:subClassOf rdf:resource="http://www.semanticwebbuilder.org/swb4/ontology#WebSite"/>
    <swb:hasHerarquicalNode rdf:resource="http://www.semanticwebbuilder.org/swb4/xforms/ontology#hn_Templates"/>
    <rdfs:label>Sitio de CPTM</rdfs:label>
    <rdf:type rdf:resource="http://www.semanticwebbuilder.org/swb4/ontology#Model"/>
    <swb:hasClass>
      <swb:Class rdf:ID="EventType">
        <swb:displayObject rdf:resource="http://www.semanticwebbuilder.org/swb4/community#do_WebPage"/>
        <rdfs:label xml:lang="es">Tipo de Evento</rdfs:label>
        <rdfs:label xml:lang="en">Event Type</rdfs:label>
        <rdfs:subClassOf rdf:resource="http://www.semanticwebbuilder.org/swb4/ontology#SWBClass"/>
        <rdfs:comment>Clase que hereda de WebPage.Mediante estas se administra el catálogo de tipos de eventos.pueden ser Ej. "Carnavales", "Congresos", etc</rdfs:comment>
        <rdfs:subClassOf rdf:resource="http://www.semanticwebbuilder.org/swb4/ontology#Descriptiveable"/>
        <rdfs:subClassOf rdf:resource="http://www.semanticwebbuilder.org/swb4/ontology#Traceable"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
      </swb:Class>
    </swb:hasClass>
    <swb:hasClass>
      <swb:Class rdf:ID="Event">
        <rdfs:subClassOf rdf:resource="http://www.semanticwebbuilder.org/swb4/ontology#Descriptiveable"/>
        <rdfs:label xml:lang="es">Evento</rdfs:label>
        <swb:displayObject rdf:resource="http://www.semanticwebbuilder.org/swb4/community#do_WebPage"/>
        <rdfs:label xml:lang="en">Event</rdfs:label>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
        <swb:autogenId rdf:datatype="http://www.w3.org/2001/XMLSchema#boolean"
        >true</swb:autogenId>
        <rdfs:comment>Clase que hereda de WebPage.Mediante estas se administra el catálogo de Eventos.</rdfs:comment>
        <rdfs:subClassOf rdf:resource="http://www.semanticwebbuilder.org/swb4/ontology#Resourceable"/>
        <rdfs:subClassOf rdf:resource="http://www.semanticwebbuilder.org/swb4/ontology#SWBClass"/>
        <rdfs:subClassOf rdf:resource="http://www.semanticwebbuilder.org/swb4/ontology#Traceable"/>
      </swb:Class>
    </swb:hasClass>
    <swb:hasHerarquicalNode rdf:resource="http://www.semanticwebbuilder.org/swb4/xforms/ontology#hn_PFlows"/>
    <swb:hasClass rdf:resource="http://www.semanticwebbuilder.org/swb4/community#MicroSite"/>
    <swb:hasHerarquicalNode rdf:resource="http://www.semanticwebbuilder.org/swb4/community#hn_MicroSiteTypes"/>
    <swb:hasClass>
      <swb:Class rdf:ID="Region">
        <rdfs:subClassOf>
          <owl:Restriction>
            <owl:allValuesFrom>
              <swb:Class rdf:ID="State">
                <rdfs:subClassOf>
                  <owl:Restriction>
                    <owl:allValuesFrom rdf:resource="#Region"/>
                    <owl:onProperty rdf:resource="http://www.semanticwebbuilder.org/swb4/ontology#webPageParent"/>
                  </owl:Restriction>
                </rdfs:subClassOf>
                <swb:displayObject rdf:resource="http://www.semanticwebbuilder.org/swb4/community#do_WebPage"/>
                <rdfs:label xml:lang="es">Estado</rdfs:label>
                <rdfs:label xml:lang="en">Stete</rdfs:label>
                <rdfs:comment>Clase que hereda de WebPage.Mediante esta se administra el catálogo de Estados de la república.</rdfs:comment>
                <rdfs:subClassOf rdf:resource="http://www.semanticwebbuilder.org/swb4/ontology#WebPage"/>
                <rdfs:subClassOf>
                  <owl:Restriction>
                    <owl:onProperty rdf:resource="http://www.semanticwebbuilder.org/swb4/ontology#hasWebPageChild"/>
                    <owl:allValuesFrom>
                      <swb:Class rdf:ID="GeographicPoint">
                        <rdfs:label xml:lang="en">Geographic Point</rdfs:label>
                        <swb:displayObject rdf:resource="http://www.semanticwebbuilder.org/swb4/community#do_WebPage"/>
                        <rdfs:subClassOf rdf:resource="http://www.semanticwebbuilder.org/swb4/ontology#Hiddenable"/>
                        <rdfs:comment>Clase que hereda de WebPage.Mediante estas se administra el catálogo de Destinos.</rdfs:comment>
                        <rdfs:subClassOf rdf:resource="http://www.semanticwebbuilder.org/swb4/ontology#WebPage"/>
                        <rdfs:label xml:lang="es">Punto geográfico</rdfs:label>
                        <rdfs:subClassOf>
                          <swb:Interface rdf:ID="LocalityInt">
                            <rdfs:comment>Interfaz que define propiedades en comun para un Punto Geografico y una Marca Regional</rdfs:comment>
                            <rdfs:subClassOf rdf:resource="http://www.semanticwebbuilder.org/swb4/ontology#SWBInterface"/>
                            <rdfs:label xml:lang="es">Localidad</rdfs:label>
                            <rdfs:label xml:lang="en">Locality</rdfs:label>
                            <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
                          </swb:Interface>
                        </rdfs:subClassOf>
                        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
                      </swb:Class>
                    </owl:allValuesFrom>
                  </owl:Restriction>
                </rdfs:subClassOf>
                <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
              </swb:Class>
            </owl:allValuesFrom>
            <owl:onProperty rdf:resource="http://www.semanticwebbuilder.org/swb4/ontology#hasWebPageChild"/>
          </owl:Restriction>
        </rdfs:subClassOf>
        <swb:displayObject rdf:resource="http://www.semanticwebbuilder.org/swb4/community#do_WebPage"/>
        <rdfs:label xml:lang="es">Region</rdfs:label>
        <rdfs:label xml:lang="en">Region</rdfs:label>
        <rdfs:comment>Clase que hereda de WebPage.Mediante estas se administra el catálogo de Regiones.</rdfs:comment>
        <rdfs:subClassOf rdf:resource="http://www.semanticwebbuilder.org/swb4/ontology#WebPage"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
      </swb:Class>
    </swb:hasClass>
    <swb:hasHerarquicalNode rdf:resource="http://www.semanticwebbuilder.org/swb4/xforms/ontology#hn_PTStrategies"/>
    <swb:hasHerarquicalNode rdf:resource="http://www.semanticwebbuilder.org/swb4/xforms/ontology#hn_Devices"/>
    <swb:displayObject rdf:resource="http://www.semanticwebbuilder.org/swb4/ontology#do_AdminWebSite"/>
    <swb:hasHerarquicalNode rdf:resource="http://www.semanticwebbuilder.org/swb4/xforms/ontology#hn_Calendars"/>
    <swb:hasClass rdf:resource="#Festivity"/>
    <swb:hasClass>
      <swb:Class rdf:ID="TravelStyle">
        <rdfs:comment>Clase que se encarga del catalogo de "Estilos de Viaje"</rdfs:comment>
        <rdfs:label xml:lang="en">Travel Style</rdfs:label>
        <swb:displayObject rdf:resource="http://www.semanticwebbuilder.org/swb4/community#do_WebPage"/>
        <rdfs:label xml:lang="es">Estilo de Viaje</rdfs:label>
        <rdfs:subClassOf rdf:resource="http://www.semanticwebbuilder.org/swb4/ontology#WebPage"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
      </swb:Class>
    </swb:hasClass>
    <swb:hasHerarquicalNode rdf:resource="http://www.semanticwebbuilder.org/swb4/community#hn_CommUtils"/>
    <swb:hasHerarquicalNode rdf:resource="http://www.semanticwebbuilder.org/swb4/xforms/ontology#hn_Dnss"/>
    <swb:hasClass rdf:resource="#TravelRoute"/>
    <swb:hasHerarquicalNode rdf:resource="http://www.semanticwebbuilder.org/swb4/xforms/ontology#hn_Roles"/>
    <swb:hasClass rdf:resource="#GeographicPoint"/>
    <swb:hasClass>
      <swb:Class rdf:ID="ExperienceType">
        <swb:autogenId rdf:datatype="http://www.w3.org/2001/XMLSchema#boolean"
        >true</swb:autogenId>
        <swb:displayObject rdf:resource="http://www.semanticwebbuilder.org/swb4/community#do_WebPage"/>
        <rdfs:subClassOf>
          <owl:Restriction>
            <owl:allValuesFrom>
              <swb:Class rdf:ID="Experience">
                <rdfs:subClassOf>
                  <owl:Restriction>
                    <owl:allValuesFrom rdf:resource="#ExperienceType"/>
                    <owl:onProperty rdf:resource="http://www.semanticwebbuilder.org/swb4/ontology#webPageParent"/>
                  </owl:Restriction>
                </rdfs:subClassOf>
                <swb:displayObject rdf:resource="http://www.semanticwebbuilder.org/swb4/community#do_WebPage"/>
                <swb:autogenId rdf:datatype="http://www.w3.org/2001/XMLSchema#boolean"
                >true</swb:autogenId>
                <rdfs:label xml:lang="es">Experiencia</rdfs:label>
                <rdfs:label xml:lang="en">Experience</rdfs:label>
                <rdfs:comment>Clase que hereda de WebPage.Mediante estas se administra el catálogo de experiencias.</rdfs:comment>
                <rdfs:subClassOf rdf:resource="http://www.semanticwebbuilder.org/swb4/ontology#WebPage"/>
                <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
              </swb:Class>
            </owl:allValuesFrom>
            <owl:onProperty rdf:resource="http://www.semanticwebbuilder.org/swb4/ontology#hasWebPageChild"/>
          </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:label xml:lang="es">Tipo de Experiencia</rdfs:label>
        <rdfs:label xml:lang="en">Experience type</rdfs:label>
        <rdfs:comment>Clase que hereda de WebPage.Mediante estas se administra el catálogo detipos de  experiencias.</rdfs:comment>
        <rdfs:subClassOf rdf:resource="http://www.semanticwebbuilder.org/swb4/ontology#WebPage"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
      </swb:Class>
    </swb:hasClass>
    <swb:hasClass rdf:resource="#Experience"/>
    <swb:hasClass>
      <swb:Class rdf:ID="Activity">
        <rdfs:label xml:lang="es">Actividad</rdfs:label>
        <rdfs:label xml:lang="en">Activity</rdfs:label>
        <rdfs:subClassOf>
          <owl:Restriction>
            <owl:allValuesFrom>
              <swb:Class rdf:ID="ActivityType">
                <rdfs:label xml:lang="es">Tipo de Actividad</rdfs:label>
                <swb:displayObject rdf:resource="http://www.semanticwebbuilder.org/swb4/community#do_WebPage"/>
                <rdfs:label xml:lang="en">Activity Type</rdfs:label>
                <rdfs:subClassOf>
                  <owl:Restriction>
                    <owl:allValuesFrom rdf:resource="#Activity"/>
                    <owl:onProperty rdf:resource="http://www.semanticwebbuilder.org/swb4/ontology#hasWebPageChild"/>
                  </owl:Restriction>
                </rdfs:subClassOf>
                <rdfs:comment>Clase que hereda de WebPage.Mediante esta se administra el catálogo de tipos de actividades.</rdfs:comment>
                <rdfs:subClassOf rdf:resource="http://www.semanticwebbuilder.org/swb4/ontology#WebPage"/>
                <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
              </swb:Class>
            </owl:allValuesFrom>
            <owl:onProperty rdf:resource="http://www.semanticwebbuilder.org/swb4/ontology#webPageParent"/>
          </owl:Restriction>
        </rdfs:subClassOf>
        <swb:displayObject rdf:resource="http://www.semanticwebbuilder.org/swb4/community#do_WebPage"/>
        <rdfs:subClassOf rdf:resource="http://www.semanticwebbuilder.org/swb4/ontology#WebPage"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
      </swb:Class>
    </swb:hasClass>
    <swb:hasClass rdf:resource="#State"/>
    <swb:hasHerarquicalNode rdf:resource="http://www.semanticwebbuilder.org/swb4/xforms/ontology#hn_Countries"/>
    <swb:hasHerarquicalNode rdf:resource="http://www.semanticwebbuilder.org/swb4/xforms/ontology#hn_PTSystems"/>
    <swb:hasHerarquicalNode rdf:resource="http://www.semanticwebbuilder.org/swb4/xforms/ontology#hn_Collections"/>
    <swb:hasClass rdf:resource="http://www.semanticwebbuilder.org/swb4/community#MicroSiteWebPageUtil"/>
    <swb:hasClass rdf:resource="#TravelRouteType"/>
    <swb:hasHerarquicalNode rdf:resource="http://www.semanticwebbuilder.org/swb4/xforms/ontology#hn_Languages"/>
    <swb:hasHerarquicalNode rdf:resource="http://www.semanticwebbuilder.org/swb4/xforms/ontology#hn_Camps"/>
    <rdfs:comment>Clase que hereda de swb:WebSite. Es un tipo de website para CPTM. De esta manera se puede contar con todos los elementos en el arbol de navegación en la administración, y otros elementos utiles para CPTM.</rdfs:comment>
    <swb:hasHerarquicalNode rdf:resource="http://www.semanticwebbuilder.org/swb4/xforms/ontology#hn_PTContents"/>
    <swb:hasClass rdf:resource="#ActivityType"/>
  </owl:Class>
  <owl:Class rdf:ID="New">
    <rdfs:subClassOf rdf:resource="http://www.semanticwebbuilder.org/swb4/ontology#Resourceable"/>
    <swb:autogenId rdf:datatype="http://www.w3.org/2001/XMLSchema#boolean"
    >true</swb:autogenId>
    <rdfs:subClassOf rdf:resource="http://www.semanticwebbuilder.org/swb4/ontology#SWBClass"/>
    <rdfs:subClassOf rdf:resource="http://www.semanticwebbuilder.org/swb4/ontology#Descriptiveable"/>
    <rdf:type rdf:resource="http://www.semanticwebbuilder.org/swb4/ontology#Class"/>
    <rdfs:subClassOf rdf:resource="http://www.semanticwebbuilder.org/swb4/ontology#Traceable"/>
    <rdfs:label xml:lang="es">Noticia</rdfs:label>
    <rdfs:comment>Catálogo de Noticias</rdfs:comment>
    <rdfs:label xml:lang="en">New</rdfs:label>
  </owl:Class>
  <owl:Class rdf:ID="TravelStyleRef">
    <rdfs:subClassOf rdf:resource="http://www.semanticwebbuilder.org/swb4/ontology#SWBClass"/>
    <rdf:type rdf:resource="http://www.semanticwebbuilder.org/swb4/ontology#Class"/>
  </owl:Class>
  <owl:Class rdf:ID="FestivityRef">
    <rdfs:subClassOf rdf:resource="http://www.semanticwebbuilder.org/swb4/ontology#SWBClass"/>
    <rdf:type rdf:resource="http://www.semanticwebbuilder.org/swb4/ontology#Class"/>
  </owl:Class>
  <owl:ObjectProperty rdf:ID="hasGeographicPointProxInv"/>
  <owl:ObjectProperty rdf:ID="tsrefLocality">
    <rdfs:domain rdf:resource="#TravelStyleRef"/>
    <rdfs:range rdf:resource="#LocalityInt"/>
    <swb:displayProperty>
      <swbxf:DisplayProperty rdf:ID="dp_tsrefLocality">
        <swb:index rdf:datatype="http://www.w3.org/2001/XMLSchema#int"
        >200</swb:index>
        <swbxf:formElement rdf:resource="http://www.semanticwebbuilder.org/swb4/xforms/ontology#selectOneModel"/>
        <swbxf:propGroup>
          <swbxf:PropertyGroup rdf:ID="others">
            <rdfs:label xml:lang="en">Other data</rdfs:label>
            <rdfs:label xml:lang="es">Otros datos</rdfs:label>
            <swb:index rdf:datatype="http://www.w3.org/2001/XMLSchema#int"
            >200</swb:index>
          </swbxf:PropertyGroup>
        </swbxf:propGroup>
      </swbxf:DisplayProperty>
    </swb:displayProperty>
    <rdfs:label xml:lang="es">Localidad</rdfs:label>
    <rdfs:label xml:lang="en">Locality</rdfs:label>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:ID="ExpLocality">
    <swb:displayProperty>
      <swbxf:DisplayProperty rdf:ID="dp_ExperienceDestinationProp">
        <swb:index rdf:datatype="http://www.w3.org/2001/XMLSchema#int"
        >200</swb:index>
        <swbxf:formElement rdf:resource="http://www.semanticwebbuilder.org/swb4/xforms/ontology#selectOneModel"/>
        <swbxf:propGroup rdf:resource="#others"/>
      </swbxf:DisplayProperty>
    </swb:displayProperty>
    <rdfs:domain rdf:resource="#ExperienceRef"/>
    <rdfs:range rdf:resource="#LocalityInt"/>
    <rdfs:label xml:lang="es">Localidad</rdfs:label>
    <rdfs:label xml:lang="en">Locality</rdfs:label>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:ID="eventGeographicPointInv"/>
  <owl:ObjectProperty rdf:ID="hasEventInv"/>
  <owl:ObjectProperty rdf:ID="hasRegGeographicPoint">
    <rdfs:domain>
      <swb:Class rdf:ID="RegionalBrand">
        <rdfs:subClassOf rdf:resource="http://www.semanticwebbuilder.org/swb4/ontology#SWBClass"/>
        <swb:displayObject rdf:resource="http://www.semanticwebbuilder.org/swb4/community#do_WebPage"/>
        <rdfs:label xml:lang="en">Regional Brand</rdfs:label>
        <rdfs:label xml:lang="es">Marca Regional</rdfs:label>
        <rdfs:comment>Marca Regional. La cual puede tener relaciodos Destinos Turisticos y Puntos geográficos (Ej. La Riviera Maya)</rdfs:comment>
        <rdfs:subClassOf rdf:resource="#LocalityInt"/>
        <rdfs:subClassOf rdf:resource="http://www.semanticwebbuilder.org/swb4/ontology#Descriptiveable"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
      </swb:Class>
    </rdfs:domain>
    <swb:displayProperty>
      <swbxf:DisplayProperty rdf:ID="dp_hasRegDestination">
        <swbxf:propGroup rdf:resource="#others"/>
        <swbxf:formElement rdf:resource="http://www.semanticwebbuilder.org/swb4/xforms/ontology#SelectMultipleModel"/>
        <swb:index rdf:datatype="http://www.w3.org/2001/XMLSchema#int"
        >200</swb:index>
      </swbxf:DisplayProperty>
    </swb:displayProperty>
    <owl:inverseOf>
      <owl:ObjectProperty rdf:ID="hasRegGeographicPointInv"/>
    </owl:inverseOf>
