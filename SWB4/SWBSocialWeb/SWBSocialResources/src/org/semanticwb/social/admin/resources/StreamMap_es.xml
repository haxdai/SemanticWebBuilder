<admresource xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
	<xslfile>admresource.xslt</xslfile>
	<form name="frmAdminSearch" action="true" method="post">
	    <admdbconnmgr defconn="true"/>
            <fieldset >
                <statictext name="used">Configuración Funcionamiento del Recurso</statictext>
                <input type="radio" id="usedas0" name="streamMapView" label="Solo Contadores:" value="1" checked="true"/>
