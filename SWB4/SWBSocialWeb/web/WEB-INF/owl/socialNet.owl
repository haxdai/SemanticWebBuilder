    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    <rdfs:domain rdf:resource="#PostOutNet"/>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:ID="pfiTime">
    <rdfs:domain rdf:resource="#SocialPFlowInstance"/>
    <swb:propertyCodeName rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >time</swb:propertyCodeName>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#dateTime"/>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:ID="socialwpPhoto">
    <swb:displayProperty>
      <swbxf:DisplayProperty rdf:ID="dp_socialwpPhoto">
        <swbxf:propGroup rdf:resource="#publish_Grp"/>
        <swbxf:formElement>
          <swb:DojoImageUpload rdf:ID="socialDojoImageUpload">
            <swbxf:fileFilter rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
