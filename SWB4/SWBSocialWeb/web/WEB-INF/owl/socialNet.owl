      </swbxf:DisplayProperty>
    </swb:displayProperty>
    <rdfs:comment xml:lang="en">Date</rdfs:comment>
    <rdfs:comment xml:lang="es">Fecha</rdfs:comment>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#date"/>
