    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    <swb:required rdf:datatype="http://www.w3.org/2001/XMLSchema#boolean"
    >false</swb:required>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:ID="status">
    <rdfs:domain rdf:resource="#PostOutNet"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#int"/>
    <rdfs:comment>Estatus del PostOut en una instancia de red social específica. 0=No publicado; 1=Publicado;</rdfs:comment>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:ID="pout_created">
    <rdfs:domain rdf:resource="#PostOut"/>
