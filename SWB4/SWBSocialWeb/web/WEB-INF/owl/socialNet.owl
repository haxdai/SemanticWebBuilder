        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
      </swb:Class>
    </rdfs:range>
    <rdfs:domain>
      <swb:Class rdf:ID="PostOut">
        <rdfs:subClassOf>
          <swb:Interface rdf:ID="SocialCalendarRefable">
            <rdfs:comment>Interfaz que define propiedades para elementos que pueden referenciar a calendarios sociales</rdfs:comment>
