        </swbxf:formElement>
        <swbxf:propInvalidMessage xml:lang="es">Este campo solo acepta números</swbxf:propInvalidMessage>
        <swbxf:propInvalidMessage xml:lang="en">This field only accepts numbers</swbxf:propInvalidMessage>
        <swbxf:propPromptMessage xml:lang="es">Este campo solo acepta números</swbxf:propPromptMessage>
        <swbxf:propPromptMessage xml:lang="en">This field only accepts numbers</swbxf:propPromptMessage>
      </swbxf:DisplayProperty>
