<?xml version="1.0"?>
<rdf:RDF
    xmlns:swbxf="http://www.semanticwebbuilder.org/swb4/xforms/ontology#"
    xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
    xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
    xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
    xmlns:owl="http://www.w3.org/2002/07/owl#"
    xmlns:swb="http://www.semanticwebbuilder.org/swb4/ontology#"
    xmlns:social="http://www.semanticwebbuilder.org/swb4/social#"
  xml:base="http://www.semanticwebbuilder.org/swb4/social">
  <owl:Ontology rdf:about="">
    <owl:imports rdf:resource="http://www.semanticwebbuilder.org/swb4/ontology"/>
    <owl:versionInfo rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >Created with TopBraid Composer</owl:versionInfo>
  </owl:Ontology>
  <owl:Class rdf:ID="PostTextable">
    <rdfs:comment>Interface que contine propiedades necesarias para el manejo de un post que contenga un texto o Mensaje.</rdfs:comment>
    <rdfs:subClassOf>
      <swb:Interface rdf:ID="PostDataable">
        <rdfs:subClassOf rdf:resource="http://www.semanticwebbuilder.org/swb4/ontology#SWBInterface"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
      </swb:Interface>
    </rdfs:subClassOf>
    <rdf:type rdf:resource="http://www.semanticwebbuilder.org/swb4/ontology#Interface"/>
  </owl:Class>
  <owl:Class rdf:ID="PostVideoable">
    <rdfs:comment>Interface que contine propiedades necesarias para el manejo de un post que contenga un Video.</rdfs:comment>
    <rdfs:subClassOf rdf:resource="#PostDataable"/>
    <rdf:type rdf:resource="http://www.semanticwebbuilder.org/swb4/ontology#Interface"/>
  </owl:Class>
  <owl:Class rdf:ID="SocialNetwork">
    <rdfs:comment>Clase que engloba a las diferentes clases que representan cada una de las redes sociales.</rdfs:comment>
    <rdf:type rdf:resource="http://www.semanticwebbuilder.org/swb4/ontology#Class"/>
    <rdfs:subClassOf rdf:resource="http://www.semanticwebbuilder.org/swb4/ontology#Descriptiveable"/>
    <rdfs:subClassOf>
      <swb:Interface rdf:ID="Secreteable">
        <rdfs:comment>Interface que contiene ciertas propiedades utiles para la autenticación de ciertas redes sociales (ver mas adelante si se unifica con Oauthable)</rdfs:comment>
        <rdfs:subClassOf rdf:resource="http://www.semanticwebbuilder.org/swb4/ontology#SWBInterface"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
      </swb:Interface>
    </rdfs:subClassOf>
    <swb:autogenId rdf:datatype="http://www.w3.org/2001/XMLSchema#boolean"
    >true</swb:autogenId>
    <rdfs:subClassOf>
      <owl:Class rdf:ID="Listenerable"/>
    </rdfs:subClassOf>
    <rdfs:subClassOf rdf:resource="http://www.semanticwebbuilder.org/swb4/ontology#SWBClass"/>
    <swb:displayObject>
      <swbxf:DisplayObject rdf:ID="do_SocialNetwork">
        <swbxf:doDispatcher rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
        >cnf_socialNetwork.zul</swbxf:doDispatcher>
        <swb:iconClass rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
        >socialnet.png</swb:iconClass>
      </swbxf:DisplayObject>
    </swb:displayObject>
    <rdfs:subClassOf rdf:resource="http://www.semanticwebbuilder.org/swb4/ontology#Activeable"/>
    <rdfs:subClassOf rdf:resource="http://www.semanticwebbuilder.org/swb4/ontology#Traceable"/>
  </owl:Class>
  <owl:Class rdf:ID="TakeOut">
    <rdfs:subClassOf rdf:resource="http://www.semanticwebbuilder.org/swb4/ontology#SWBClass"/>
    <rdfs:comment>Clase que englobara elementos a ser quitados en los mensajes de las diferentes redes sociales, de tal manera que se puedan comparar cada uno de las palabras de los mensajes con las palabras de valor sentimental que se tengan almacenadas en la clase SentimentalWords.</rdfs:comment>
    <rdf:type rdf:resource="http://www.semanticwebbuilder.org/swb4/ontology#Class"/>
  </owl:Class>
  <owl:Class rdf:ID="DeleteControlable">
    <rdfs:subClassOf rdf:resource="http://www.semanticwebbuilder.org/swb4/ontology#SWBInterface"/>
    <rdfs:comment>Interface que define que las instancias de una clase pueden ser eliminadas</rdfs:comment>
    <rdf:type rdf:resource="http://www.semanticwebbuilder.org/swb4/ontology#Interface"/>
  </owl:Class>
  <owl:Class rdf:ID="Stream">
    <rdfs:subClassOf rdf:resource="http://www.w3.org/2000/01/rdf-schema#Resource"/>
    <rdfs:comment>Clase que contendra los streams que configurados para cada usuario</rdfs:comment>
    <swb:autogenId rdf:datatype="http://www.w3.org/2001/XMLSchema#boolean"
    >true</swb:autogenId>
    <rdfs:subClassOf rdf:resource="http://www.semanticwebbuilder.org/swb4/ontology#Traceable"/>
    <rdfs:subClassOf>
      <swb:Interface rdf:ID="Geolocable">
        <rdfs:comment>Interface que contiene propiedades de localización que seran aplicadas a los mensajes que lleguen desde las redes sociales</rdfs:comment>
        <rdfs:subClassOf rdf:resource="http://www.semanticwebbuilder.org/swb4/ontology#SWBInterface"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
      </swb:Interface>
    </rdfs:subClassOf>
    <rdfs:subClassOf rdf:resource="http://www.semanticwebbuilder.org/swb4/ontology#Activeable"/>
    <swb:displayObject>
      <swbxf:DisplayObject rdf:ID="do_Stream">
        <swb:iconClass rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
        >stream.png</swb:iconClass>
      </swbxf:DisplayObject>
    </swb:displayObject>
    <rdfs:subClassOf rdf:resource="http://www.semanticwebbuilder.org/swb4/ontology#Descriptiveable"/>
    <rdfs:subClassOf rdf:resource="http://www.semanticwebbuilder.org/swb4/ontology#SWBClass"/>
    <rdf:type rdf:resource="http://www.semanticwebbuilder.org/swb4/ontology#Class"/>
  </owl:Class>
  <owl:Class rdf:ID="SocialTopic">
    <swb:autogenId rdf:datatype="http://www.w3.org/2001/XMLSchema#boolean"
    >true</swb:autogenId>
    <swb:displayObject>
      <swbxf:DisplayObject rdf:ID="do_SocialTopic">
        <swb:iconClass rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
        >socialnet.png</swb:iconClass>
      </swbxf:DisplayObject>
    </swb:displayObject>
    <rdfs:subClassOf>
      <swb:Interface rdf:ID="Childrenable">
        <rdfs:comment>Interface para las clases que apareceran en el árbol y que las instancias de esas clases podran tener a su vez instancias de las mismas</rdfs:comment>
        <rdfs:subClassOf rdf:resource="http://www.semanticwebbuilder.org/swb4/ontology#SWBInterface"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
      </swb:Interface>
    </rdfs:subClassOf>
    <rdfs:subClassOf rdf:resource="http://www.semanticwebbuilder.org/swb4/ontology#SWBClass"/>
    <rdfs:comment>Catalogo de temas de un modelo (Marca)</rdfs:comment>
    <rdfs:subClassOf rdf:resource="http://www.semanticwebbuilder.org/swb4/ontology#Descriptiveable"/>
    <rdfs:subClassOf rdf:resource="http://www.semanticwebbuilder.org/swb4/ontology#Traceable"/>
    <rdf:type rdf:resource="http://www.semanticwebbuilder.org/swb4/ontology#Class"/>
  </owl:Class>
  <owl:Class rdf:about="#Listenerable">
    <rdfs:subClassOf rdf:resource="http://www.semanticwebbuilder.org/swb4/ontology#SWBInterface"/>
    <rdfs:comment>Esta interfaz se le agrega a las clases de tipo SocialNetwork que se va ha desear que escuchen en las redes sociales, lo cual en teoría deberían de ser a todas, pero por si en un futuro de desea que no sea así.</rdfs:comment>
    <rdf:type rdf:resource="http://www.semanticwebbuilder.org/swb4/ontology#Interface"/>
  </owl:Class>
  <owl:Class rdf:ID="TreeNodePage">
    <rdfs:subClassOf>
      <swb:Class rdf:ID="ZulWebPage">
        <swb:displayObject rdf:resource="http://www.semanticwebbuilder.org/swb4/xforms/ontology#do_WebPage"/>
        <rdfs:label xml:lang="en">Zul Menu Page</rdfs:label>
        <rdfs:label xml:lang="es">Página de Menu tipo Zul</rdfs:label>
        <rdfs:subClassOf rdf:resource="http://www.semanticwebbuilder.org/swb4/ontology#WebPage"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
      </swb:Class>
    </rdfs:subClassOf>
    <swb:displayObject rdf:resource="http://www.semanticwebbuilder.org/swb4/xforms/ontology#do_WebPage"/>
    <rdfs:label xml:lang="en">Tree Category nodo Page</rdfs:label>
    <rdfs:label xml:lang="es">Página de nodo en el Árbol de Navegación</rdfs:label>
    <rdf:type rdf:resource="http://www.semanticwebbuilder.org/swb4/ontology#Class"/>
  </owl:Class>
  <owl:Class rdf:ID="SocialNetPostable">
    <rdfs:comment>Interface que agrupa interfaces utiles para manejo de cada uno de los tipos de post que pueden ser enviados y recibidos en las diferentes redes sociales.</rdfs:comment>
    <rdfs:subClassOf rdf:resource="http://www.semanticwebbuilder.org/swb4/ontology#SWBInterface"/>
    <rdf:type rdf:resource="http://www.semanticwebbuilder.org/swb4/ontology#Interface"/>
  </owl:Class>
  <owl:Class rdf:ID="SocialSite">
    <rdfs:label xml:lang="es">Sitio Social</rdfs:label>
    <swb:hasHerarquicalNode rdf:resource="http://www.semanticwebbuilder.org/swb4/xforms/ontology#hn_Dnss"/>
    <swb:displayObject>
      <swbxf:DisplayObject rdf:ID="do_SocialSite">
        <swb:iconClass rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
        >swbIconWebSite</swb:iconClass>
        <swbxf:doDispatcher rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
        >socialsite.png</swbxf:doDispatcher>
      </swbxf:DisplayObject>
    </swb:displayObject>
    <swb:hasHerarquicalNode rdf:resource="http://www.semanticwebbuilder.org/swb4/xforms/ontology#hn_Languages"/>
    <swb:hasHerarquicalNode rdf:resource="http://www.semanticwebbuilder.org/swb4/xforms/ontology#hn_Roles"/>
    <swb:hasHerarquicalNode rdf:resource="http://www.semanticwebbuilder.org/swb4/xforms/ontology#hn_Calendars"/>
    <swb:hasHerarquicalNode rdf:resource="http://www.semanticwebbuilder.org/swb4/xforms/ontology#hn_Countries"/>
    <swb:hasHerarquicalNode rdf:resource="http://www.semanticwebbuilder.org/swb4/xforms/ontology#hn_Templates"/>
    <rdfs:subClassOf rdf:resource="http://www.semanticwebbuilder.org/swb4/ontology#WebSite"/>
    <swb:hasHerarquicalNode rdf:resource="http://www.semanticwebbuilder.org/swb4/xforms/ontology#hn_PTStrategies"/>
    <swb:hasHerarquicalNode rdf:resource="http://www.semanticwebbuilder.org/swb4/xforms/ontology#hn_Collections"/>
    <swb:hasHerarquicalNode rdf:resource="http://www.semanticwebbuilder.org/swb4/xforms/ontology#hn_UserGroups"/>
    <swb:hasHerarquicalNode rdf:resource="http://www.semanticwebbuilder.org/swb4/xforms/ontology#hn_PTContents"/>
    <rdfs:label xml:lang="en">Social Site</rdfs:label>
    <rdfs:comment>Clase que hereda de swb:WebSite. Es un tipo de website Social. De esta manera se puede contar con todos los elementos en el arbol de navegación en la administración, y otros elementos utiles para Social Site.</rdfs:comment>
    <swb:hasHerarquicalNode rdf:resource="http://www.semanticwebbuilder.org/swb4/xforms/ontology#hn_PTSystems"/>
    <rdf:type rdf:resource="http://www.semanticwebbuilder.org/swb4/ontology#Model"/>
    <swb:hasHerarquicalNode rdf:resource="http://www.semanticwebbuilder.org/swb4/xforms/ontology#hn_PFlows"/>
    <swb:hasHerarquicalNode rdf:resource="http://www.semanticwebbuilder.org/swb4/xforms/ontology#hn_Camps"/>
    <swb:hasHerarquicalNode rdf:resource="http://www.semanticwebbuilder.org/swb4/xforms/ontology#hn_Rules"/>
    <swb:hasHerarquicalNode rdf:resource="http://www.semanticwebbuilder.org/swb4/xforms/ontology#hn_Devices"/>
  </owl:Class>
  <owl:Class rdf:ID="Kloutable">
    <rdfs:subClassOf rdf:resource="http://www.semanticwebbuilder.org/swb4/ontology#SWBInterface"/>
    <rdfs:comment>Interface que se les agrega a las redes sociales a las cuales se pueda leer el klout de sus usuarios. Segun Klout, en este momento solo soporta twitter y google+</rdfs:comment>
    <rdf:type rdf:resource="http://www.semanticwebbuilder.org/swb4/ontology#Interface"/>
  </owl:Class>
  <owl:Class rdf:ID="SocialAdmin">
    <swb:hasHerarquicalNode rdf:resource="http://www.semanticwebbuilder.org/swb4/xforms/ontology#hn_Templates"/>
    <swb:hasHerarquicalNode rdf:resource="http://www.semanticwebbuilder.org/swb4/xforms/ontology#hn_Dnss"/>
    <swb:displayObject rdf:resource="http://www.semanticwebbuilder.org/swb4/ontology#do_AdminWebSite"/>
    <rdfs:subClassOf>
      <rdf:Description rdf:about="http://www.semanticwebbuilder.org/swb4/ontology#AdminWebSite">
        <swb:hasHerarquicalNode rdf:resource="http://www.semanticwebbuilder.org/swb4/xforms/ontology#hn_Collections"/>
      </rdf:Description>
    </rdfs:subClassOf>
    <swb:hasClass rdf:resource="#TreeNodePage"/>
    <rdfs:label xml:lang="en">Social Admin WebSite</rdfs:label>
    <swb:hasHerarquicalNode rdf:resource="http://www.semanticwebbuilder.org/swb4/xforms/ontology#hn_PTContents"/>
    <swb:hasHerarquicalNode rdf:resource="http://www.semanticwebbuilder.org/swb4/xforms/ontology#hn_Collections"/>
    <swb:hasClass rdf:resource="#ZulWebPage"/>
    <swb:hasClass rdf:resource="http://www.semanticwebbuilder.org/swb4/xforms/ontology#ObjectBehavior"/>
    <rdfs:comment>Objeto que define un Sitio Web de Administración de SWBSocial</rdfs:comment>
    <rdf:type rdf:resource="http://www.semanticwebbuilder.org/swb4/ontology#Model"/>
    <swb:hasClass rdf:resource="http://www.semanticwebbuilder.org/swb4/xforms/ontology#MenuItem"/>
    <rdfs:label xml:lang="es">Sitio de administración Social</rdfs:label>
    <swb:hasClass rdf:resource="http://www.semanticwebbuilder.org/swb4/ontology#Collection"/>
  </owl:Class>
  <owl:Class rdf:ID="PublishFlow">
    <rdfs:subClassOf rdf:resource="http://www.semanticwebbuilder.org/swb4/ontology#SWBClass"/>
    <rdfs:subClassOf rdf:resource="http://www.semanticwebbuilder.org/swb4/ontology#Traceable"/>
    <rdfs:subClassOf rdf:resource="http://www.semanticwebbuilder.org/swb4/ontology#Descriptiveable"/>
    <rdf:type rdf:resource="http://www.semanticwebbuilder.org/swb4/ontology#Class"/>
  </owl:Class>
  <owl:Class rdf:ID="PostImageable">
    <rdfs:comment>Interface que contine propiedades necesarias para el manejo de un post que contenga una imagen o foto.</rdfs:comment>
    <rdfs:subClassOf rdf:resource="#PostDataable"/>
    <rdf:type rdf:resource="http://www.semanticwebbuilder.org/swb4/ontology#Interface"/>
  </owl:Class>
  <owl:ObjectProperty rdf:ID="postListenerBase">
    <owl:inverseOf>
      <owl:ObjectProperty rdf:ID="hasNewCreatedPostInv"/>
    </owl:inverseOf>
    <rdfs:range>
      <swb:Class rdf:ID="PostListenerContainerBase">
        <rdfs:subClassOf rdf:resource="http://www.semanticwebbuilder.org/swb4/ontology#Traceable"/>
        <rdfs:subClassOf rdf:resource="http://www.semanticwebbuilder.org/swb4/ontology#SWBClass"/>
        <rdfs:comment>Clase a Cambiar despues por "Relacional".  Clase que va ha contener los Post que han sido tomados como base (es decir, que llegan por el listener y que se guardan en la clase PostListenerContainer) para crear un nuevo Post desde la herramienta y que se envía hacia las redes sociales.Si se eliminan un post que han sifo tomados como base(PostIn), se debe de eliminar la instancia asociada de esta clase (en la propiedad plcb_Post).</rdfs:comment>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
      </swb:Class>
    </rdfs:range>
    <rdfs:comment>En esta propiedad se guarda el post que llegó por el listener y que sirvió de base para que se creara un post nuevo desde SSMCC.</rdfs:comment>
    <rdfs:domain>
      <swb:Class rdf:ID="PostOut">
        <rdfs:subClassOf>
          <swb:Class rdf:ID="Post">
            <rdfs:subClassOf rdf:resource="http://www.semanticwebbuilder.org/swb4/ontology#Traceable"/>
            <rdfs:subClassOf rdf:resource="http://www.semanticwebbuilder.org/swb4/ontology#Tagable"/>
            <swb:autogenId rdf:datatype="http://www.w3.org/2001/XMLSchema#boolean"
            >true</swb:autogenId>
            <rdfs:subClassOf rdf:resource="http://www.semanticwebbuilder.org/swb4/ontology#SWBClass"/>
            <rdfs:comment>Clase a Cambiar despues por "Relacional", esta y todas sus hijas. Clase que comprende todos los tipos de Post que pueden ir siendo creados en la herramienta..</rdfs:comment>
            <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
          </swb:Class>
        </rdfs:subClassOf>
        <rdfs:comment>Clase que comprende todos los tipos de Post de Salida que pueden ir siendo creados en la herramienta y que seran publicados a partir de esto en las diferentes redes sociales.</rdfs:comment>
        <rdfs:subClassOf rdf:resource="http://www.semanticwebbuilder.org/swb4/ontology#Descriptiveable"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
      </swb:Class>
    </rdfs:domain>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:ID="hasUser">
    <owl:inverseOf>
      <owl:ObjectProperty rdf:ID="hasPublishFlowInv"/>
    </owl:inverseOf>
    <rdfs:comment>Un flujo de publicación puede estar asignado a mas de un usuario. Esto es para que los usuarios sigan uno o mas flujos.</rdfs:comment>
    <rdfs:range rdf:resource="http://www.semanticwebbuilder.org/swb4/ontology#User"/>
    <rdfs:domain rdf:resource="#PublishFlow"/>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:ID="hasPostContainer_PostInv">
    <rdfs:comment>Propiedad inversa que me regresa los postContainers con los cuales tiene referencia un objeto(instancia) PostOut, recordemos que se crea una instancia de PostContainer por una red social, de modo que si se envía un postOut a varias redes sociales(lo cual puede pasar) pues se deverían de crear varios postContainers (Si es que no hay ya creados) por cada una de las estas redes sociales a las cuales se envía el PostOut.</rdfs:comment>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:ID="hasPostInv">
    <swb:propertyCodeName rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >hasPost</swb:propertyCodeName>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:ID="socialNetwork">
    <rdfs:comment>Objeto Red Social a la cual se hace referencia.</rdfs:comment>
    <rdfs:range rdf:resource="#SocialNetwork"/>
    <rdfs:domain>
      <swb:Class rdf:ID="SocialPost">
        <rdfs:comment>En este objeto se guardara el identificador que es asignado para cada post en cada una de las redes sociales, es decir, si un mismo post se envía hacia mas de una red social, cada una de esas redes sociales daran un identificador unico para ese post en esa red social, este lo tenemos que guardar nosotros en este objeto para fines de monitoreo de estatus del post en esa red social (En Proceso, Revisado, Publicado, etc), como nosotros para un post, independientemente de a cuantas redes sociales se envíe, solo creamos un objeto Post (Message, Photo, Video), tuvimos que crear esta clase para guardar el identificador de ese post para c/red social. En el ID de este objeto se colocara el id de ese post en esa red social.</rdfs:comment>
        <swb:autogenId rdf:datatype="http://www.w3.org/2001/XMLSchema#boolean"
        >false</swb:autogenId>
        <rdfs:subClassOf rdf:resource="http://www.semanticwebbuilder.org/swb4/ontology#SWBClass"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
      </swb:Class>
    </rdfs:domain>
    <owl:inverseOf>
      <owl:ObjectProperty rdf:ID="hasSocialPostInv"/>
    </owl:inverseOf>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:ID="pc_SocialNetworkInv">
    <rdfs:comment>Con esta propiedad inversa, se puede saber a que red social pertenece este grupo de post agrupados por mes y año</rdfs:comment>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:ID="hasPostInSocialNetworkInv">
    <rdfs:comment>Esta propiedad inversa me puede decir cuales son los mensajes que han llegado por el Listener por una determinada red social.</rdfs:comment>
    <swb:removeDependency rdf:datatype="http://www.w3.org/2001/XMLSchema#boolean"
    >true</swb:removeDependency>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:ID="plcb_SocialNetwork">
    <rdfs:comment>Cuenta de Red Social a la que pertenece el post que llega por el Listener y que es tomado como base para crear un nuevo post desde la herramienta y que sera enviado a una o mas redes sociales.</rdfs:comment>
    <rdfs:range rdf:resource="#SocialNetwork"/>
    <rdfs:domain rdf:resource="#PostListenerContainerBase"/>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:ID="postInSocialNetwork">
    <owl:inverseOf rdf:resource="#hasPostInSocialNetworkInv"/>
    <rdfs:comment>Red Social de la cual es procedente el mensaje que llega por el Listener.</rdfs:comment>
    <rdfs:range rdf:resource="#SocialNetwork"/>
    <rdfs:domain>
      <swb:Class rdf:ID="PostIn">
        <rdfs:subClassOf rdf:resource="#Post"/>
        <rdfs:comment>Clase que comprende todos los tipos de Post de entrada (Povientes del Listener)que pueden ir siendo creados en la herramienta.</rdfs:comment>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
      </swb:Class>
    </rdfs:domain>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:ID="parentSocialTopic">
    <rdfs:domain rdf:resource="#SocialTopic"/>
    <rdfs:range rdf:resource="#SocialTopic"/>
    <owl:inverseOf>
      <owl:ObjectProperty rdf:ID="hasChildSocialTopicInv"/>
    </owl:inverseOf>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:ID="socialPost">
    <rdfs:comment>Objeto Post al cual se hace referencia.</rdfs:comment>
    <rdfs:range rdf:resource="#Post"/>
    <rdfs:domain rdf:resource="#SocialPost"/>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:ID="plc_SocialNetworkInv">
    <rdfs:comment>Cuenta de Red Social a la que pertenece el grupo de postIn que llega por el Listener y que son agrupados por mes y año.</rdfs:comment>
    <rdfs:range rdf:resource="#SocialNetwork"/>
    <rdfs:domain>
      <swb:Class rdf:ID="PostListenerContainer">
        <rdfs:subClassOf rdf:resource="http://www.semanticwebbuilder.org/swb4/ontology#Traceable"/>
        <swb:autogenId rdf:datatype="http://www.w3.org/2001/XMLSchema#boolean"
        >true</swb:autogenId>
        <rdfs:subClassOf rdf:resource="http://www.semanticwebbuilder.org/swb4/ontology#Descriptiveable"/>
        <rdfs:subClassOf rdf:resource="http://www.semanticwebbuilder.org/swb4/ontology#SWBClass"/>
        <rdfs:comment>Clase a Cambiar despues por "Relacional".  En esta clase se guardan todos los post que lleguan por el listener, se estima que toda la info. que se guarde en este objeto debe de eliminarse aproximadamente c/mes, siendo este parametro configurable de acuerdo al tiempo que la organización quiera guardar  la información sobre los mensajes que lleguen por el listener. Cuando un post que llegue por el listener sea tomado como base para crear un nuevo post por la organización, se cree que debe copiarse la información de dicho post de esta clase hacia la clase PostListenerContainerBase.</rdfs:comment>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
      </swb:Class>
    </rdfs:domain>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:ID="hasPostListenerContainer">
    <rdfs:comment>Con esta propiedad se puede obtener cuales son los objetos de tipo "PostListenerContainer" que tiene una determinada red social, de esta manera se pudiera saber cuales son los post que han sido recibidos desde una determinada cuenta de una red social, siendo agrupados por año y mes.</rdfs:comment>
    <owl:inverseOf rdf:resource="#plc_SocialNetworkInv"/>
    <rdfs:range rdf:resource="#PostListenerContainer"/>
    <rdfs:domain rdf:resource="#SocialNetwork"/>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:ID="hasChildrenObjInv"/>
  <owl:ObjectProperty rdf:ID="hasStream_socialNetwork">
    <swb:propertyCodeName rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >hasSocialNetwork</swb:propertyCodeName>
    <rdfs:label xml:lang="es">Redes Sociales</rdfs:label>
    <swb:displayProperty>
      <swbxf:DisplayProperty rdf:ID="dp_hasStream_socialNetwork">
        <swbxf:propGroup>
          <swbxf:PropertyGroup rdf:ID="pg_socialConfig">
            <rdfs:label xml:lang="es">Configuración</rdfs:label>
            <rdfs:label xml:lang="en">Configuration</rdfs:label>
            <rdfs:comment>Grupo de Configuración</rdfs:comment>
            <swb:index rdf:datatype="http://www.w3.org/2001/XMLSchema#int"
            >100</swb:index>
          </swbxf:PropertyGroup>
        </swbxf:propGroup>
        <swbxf:formElement rdf:resource="http://www.semanticwebbuilder.org/swb4/xforms/ontology#SelectMultipleModelWithBlank"/>
        <swb:index rdf:datatype="http://www.w3.org/2001/XMLSchema#int"
        >30</swb:index>
      </swbxf:DisplayProperty>
    </swb:displayProperty>
    <rdfs:label xml:lang="en">Social Networks</rdfs:label>
    <rdfs:domain rdf:resource="#Stream"/>
    <rdfs:range rdf:resource="#SocialNetwork"/>
    <rdfs:comment>Redes sociales asociadas al stream. En estas redes sociales se escuchara la frase asociada a un stream.</rdfs:comment>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:ID="topicPublishFlow">
    <rdfs:domain rdf:resource="#SocialTopic"/>
    <rdfs:range rdf:resource="#PublishFlow"/>
    <swb:propertyCodeName rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >publishFlow</swb:propertyCodeName>
    <owl:inverseOf>
      <owl:ObjectProperty rdf:ID="hasSocialTopicInv"/>
    </owl:inverseOf>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:ID="hasPlc_Post">
    <rdfs:comment>Referencia a los Post que llegan por el Listener y que son referenciados desde esta para que puedan ser mas facilmente encontrados, debido a que esta clase (PostListenerContainer) tiene una propiedad de año y mes en los cuales pueden ser buscados dichos post.</rdfs:comment>
    <swb:removeDependency rdf:datatype="http://www.w3.org/2001/XMLSchema#boolean"
    >true</swb:removeDependency>
    <rdfs:domain rdf:resource="#PostListenerContainer"/>
    <rdfs:range rdf:resource="#PostIn"/>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:ID="hasSocialNetworkUsersInv">
    <rdfs:comment>Propiedad inversa la cual regresa las instancias de SocialNetworkUser, es decir, los usuarios creadores de Post que entran por el Listener (PostIn) y que pertenecen a una determinada red social.</rdfs:comment>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:ID="snu_SocialNetwork">
    <rdfs:domain>
      <swb:Class rdf:ID="SocialNetworkUser">
        <rdfs:subClassOf rdf:resource="http://www.semanticwebbuilder.org/swb4/ontology#SWBClass"/>
        <swb:autogenId rdf:datatype="http://www.w3.org/2001/XMLSchema#boolean"
        >true</swb:autogenId>
        <rdfs:subClassOf rdf:resource="http://www.semanticwebbuilder.org/swb4/ontology#Traceable"/>
        <rdfs:comment>Clase en la cual se almacenan los usuarios que escriben los PostIn que llegan. No se puso como identificador de las instancias de esta clase el id que maneja el usuario en la red social, ya que un identificador de una red social, puede ser el mismo para otra red social, pero obviamnete para otro usuario.Es por ello que se puso como AutoGenID esta clase y por ello se maneja por separado el id de un usuario en una determinada red social, esto en la propiedad snu_id.</rdfs:comment>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
      </swb:Class>
    </rdfs:domain>
    <owl:inverseOf rdf:resource="#hasSocialNetworkUsersInv"/>
    <rdfs:comment>Red social a la cual pertenece este usuario (usuario de red social)</rdfs:comment>
    <rdfs:range rdf:resource="http://www.semanticwebbuilder.org/swb4/ontology#Class"/>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="#hasNewCreatedPostInv">
    <rdfs:comment>Propiedad inversa que nos proporciona todos los post que han sido creados desde el SSMCC en cuencecuencia de una instancia de Post que llega por el listener, es decir, de un post que es tomado como base para crear nuevos post, estos post que son tomados como base, estan en la clase PostListenerContainerBase (Osea en esta)</rdfs:comment>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:ID="parentObj">
    <swb:displayProperty>
      <swbxf:DisplayProperty rdf:ID="dp_BORRAR_parentObj">
        <swbxf:propGroup rdf:resource="#pg_socialConfig"/>
        <swbxf:formElement rdf:resource="http://www.semanticwebbuilder.org/swb4/xforms/ontology#selectOneModelWithBlank"/>
        <swb:index rdf:datatype="http://www.w3.org/2001/XMLSchema#int"
        >10</swb:index>
      </swbxf:DisplayProperty>
    </swb:displayProperty>
    <owl:inverseOf rdf:resource="#hasChildrenObjInv"/>
    <rdfs:range rdf:resource="#Childrenable"/>
    <rdfs:domain rdf:resource="#Childrenable"/>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:ID="hasPostInInv">
    <rdfs:comment>Propiedad inversa que regresa los PostIn pertenecientes a un usuario de una red social.</rdfs:comment>
    <swb:removeDependency rdf:datatype="http://www.w3.org/2001/XMLSchema#boolean"
    >true</swb:removeDependency>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:ID="hasPostContainer">
    <rdfs:comment>Con esta propiedad se puede obtener cuales son los objetos de tipo "PostContainer" que tiene una determinada red social, de esta manera se pudiera saber cuales son los post que han sido enviados a una determinada cuenta de una red social, siendo agrupados por año y mes.</rdfs:comment>
    <owl:inverseOf rdf:resource="#pc_SocialNetworkInv"/>
    <rdfs:range>
      <swb:Class rdf:ID="PostContainer">
        <swb:autogenId rdf:datatype="http://www.w3.org/2001/XMLSchema#boolean"
        >true</swb:autogenId>
        <rdfs:subClassOf rdf:resource="http://www.semanticwebbuilder.org/swb4/ontology#Traceable"/>
        <rdfs:subClassOf rdf:resource="http://www.semanticwebbuilder.org/swb4/ontology#SWBClass"/>
        <rdfs:comment>Clase a Cambiar despues por "Relacional". Clase que contiene todos los post que han sido enviados a una determinada red social. La intención de crear esta clase es para que se agrupen los Post de cada red social por mes y año, y de esta manera sea mucho mas sencillo, optimo y rapido realizar las busquedas.</rdfs:comment>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
      </swb:Class>
    </rdfs:range>
    <rdfs:domain rdf:resource="#SocialNetwork"/>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:ID="postInStream">
    <rdfs:domain rdf:resource="#PostIn"/>
    <rdfs:comment>Stream por el que llega el mensaje de entrada</rdfs:comment>
    <rdfs:range rdf:resource="#Stream"/>
    <owl:inverseOf>
      <owl:ObjectProperty rdf:ID="hasPostIntInv"/>
    </owl:inverseOf>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:ID="socialTopic">
    <rdfs:domain rdf:resource="#Post"/>
    <rdfs:range rdf:resource="#SocialTopic"/>
    <owl:inverseOf rdf:resource="#hasPostInv"/>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="#hasSocialTopicInv">
    <swb:propertyCodeName rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >hasSocialTopic</swb:propertyCodeName>
    <rdfs:comment>Inversa con la que obtenemos los temas a los que esta asociados un determinado flujo</rdfs:comment>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="#hasPublishFlowInv">
    <rdfs:comment>Un usuario puede tener mas de un solo flujo de publicación asignado a el.</rdfs:comment>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="#hasSocialPostInv">
    <swb:removeDependency rdf:datatype="http://www.w3.org/2001/XMLSchema#boolean"
    >true</swb:removeDependency>
    <rdfs:comment>Con esta inversa, Cuando se elimine una red social, se eliminaran todos los objetos de tipo SocialPost que este asociados a la misma.</rdfs:comment>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:ID="plcb_Post">
    <rdfs:range rdf:resource="#PostIn"/>
    <rdfs:domain rdf:resource="#PostListenerContainerBase"/>
    <rdfs:comment>Referencia al Post que es creado por un post que llega por el Listener  y que es tomado como base para crear nuevos post desde la herramienta y que sera o seran enviados a una o mas redes sociales.</rdfs:comment>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="#hasPostIntInv">
    <swb:removeDependency rdf:datatype="http://www.w3.org/2001/XMLSchema#boolean"
    >true</swb:removeDependency>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:ID="hasPost">
    <rdfs:comment>Post de Salida (PostOut)</rdfs:comment>
    <rdfs:range rdf:resource="#PostOut"/>
    <owl:inverseOf rdf:resource="#hasPostContainer_PostInv"/>
    <rdfs:domain rdf:resource="#PostContainer"/>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:ID="postInSocialNetworkUser">
    <owl:inverseOf rdf:resource="#hasPostInInv"/>
    <rdfs:domain rdf:resource="#PostIn"/>
    <rdfs:range rdf:resource="#SocialNetworkUser"/>
    <rdfs:comment>Usuario creador del Post que llega por el Listener (PostIn). TODO: Al borrar un PostIn, no borro al SocialNetworkUser porque este podría obviamente tenear mas PostIn asociados. Tengo que despues revisar para que si ya un usuario no tiene PostIn(Porque x algun modo se hayan borrado) borre al SocialNetworkUser, ya que de no ser así se quedaran como basura en la BD, haciendo que esta cresca muchisimo.</rdfs:comment>
  </owl:ObjectProperty>
  <owl:DatatypeProperty rdf:ID="ProductsAndServices">
    <swb:displayProperty>
      <swbxf:DisplayProperty rdf:ID="dp_ProductsAndServices">
        <swbxf:propGroup rdf:resource="#pg_socialConfig"/>
        <swbxf:formElement rdf:resource="http://www.semanticwebbuilder.org/swb4/xforms/ontology#TextBox"/>
        <swb:index rdf:datatype="http://www.w3.org/2001/XMLSchema#int"
        >30</swb:index>
      </swbxf:DisplayProperty>
    </swb:displayProperty>
    <rdfs:label xml:lang="en">Products &amp; Services</rdfs:label>
    <rdfs:label xml:lang="es">Productos y Servicios</rdfs:label>
    <rdfs:comment>Palabras a monitorear acerca de los productos y servicios que ofresca la compañia</rdfs:comment>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    <rdfs:domain>
      <swb:Class rdf:ID="WordsToMonitor">
        <rdfs:subClassOf rdf:resource="http://www.semanticwebbuilder.org/swb4/ontology#Traceable"/>
        <rdfs:subClassOf rdf:resource="http://www.semanticwebbuilder.org/swb4/ontology#Descriptiveable"/>
        <swb:autogenId rdf:datatype="http://www.w3.org/2001/XMLSchema#boolean"
        >true</swb:autogenId>
        <rdfs:label xml:lang="en">Words to Monitor</rdfs:label>
        <rdfs:label xml:lang="es">Palabras a Monitorear</rdfs:label>
        <rdfs:comment>Clase en donde se almacenan las palabras que se desean monitorear</rdfs:comment>
        <rdfs:subClassOf rdf:resource="http://www.semanticwebbuilder.org/swb4/ontology#SWBClass"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
      </swb:Class>
    </rdfs:domain>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:ID="postIntensityValue">
    <rdfs:comment>Valor que es resultado del algoritmo de intensidad, mediante este valor se puede determinar si la intencidad es alta, media o baja</rdfs:comment>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#float"/>
    <rdfs:domain rdf:resource="#PostIn"/>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:ID="filterSentimentalNegative">
    <rdfs:comment>Propiedad que indica si en el stream se desea aceptar que entren los mensajes que sean clasificados con sentimiento negativo</rdfs:comment>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#boolean"/>
    <rdfs:domain rdf:resource="#Stream"/>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:ID="postIntesityType">
    <swb:displayProperty>
      <swbxf:DisplayProperty rdf:ID="dp_company">
        <swbxf:propGroup rdf:resource="#pg_socialConfig"/>
        <swb:index rdf:datatype="http://www.w3.org/2001/XMLSchema#int"
        >10</swb:index>
        <swbxf:formElement rdf:resource="http://www.semanticwebbuilder.org/swb4/xforms/ontology#TextBox"/>
      </swbxf:DisplayProperty>
    </swb:displayProperty>
    <rdfs:comment>Tipo de Intensidad. 2=Alta;1=Media;0=Baja;</rdfs:comment>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#int"/>
    <rdfs:domain rdf:resource="#PostIn"/>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:ID="filterIntensityLow">
    <rdfs:domain rdf:resource="#Stream"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#boolean"/>
    <rdfs:comment>Propiedad que indica si en el stream se desea aceptar que entren los mensajes que sean clasificados con intensidad baja</rdfs:comment>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:ID="OtherWords">
    <swb:displayProperty>
      <swbxf:DisplayProperty rdf:ID="dp_OtherWords">
        <swbxf:propGroup rdf:resource="#pg_socialConfig"/>
        <swbxf:formElement rdf:resource="http://www.semanticwebbuilder.org/swb4/xforms/ontology#TextBox"/>
        <swb:index rdf:datatype="http://www.w3.org/2001/XMLSchema#int"
        >40</swb:index>
      </swbxf:DisplayProperty>
    </swb:displayProperty>
    <rdfs:label xml:lang="en">Other words</rdfs:label>
    <rdfs:label xml:lang="es">Otras palabras</rdfs:label>
    <rdfs:comment>Algunas otras palabras que se deseen monitorear</rdfs:comment>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    <rdfs:domain rdf:resource="#WordsToMonitor"/>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:ID="postSource">
    <rdfs:domain rdf:resource="#PostIn"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    <rdfs:comment>Dispositivo desde el que se ha enviado el post</rdfs:comment>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:ID="company">
    <swb:displayProperty rdf:resource="#dp_company"/>
    <rdfs:label xml:lang="en">Company</rdfs:label>
    <rdfs:label xml:lang="es">Empresa</rdfs:label>
    <rdfs:comment>Palabras a monitorear acerca de la compañia</rdfs:comment>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    <rdfs:domain rdf:resource="#WordsToMonitor"/>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:ID="filterSentimentalPositive">
    <rdfs:comment>Propiedad que indica si en el stream se desea aceptar que entren los mensajes que sean clasificados con sentimiento positivo</rdfs:comment>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#boolean"/>
    <rdfs:domain rdf:resource="#Stream"/>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:ID="postSentimentalValue">
    <rdfs:comment>Valor que resulta del algoritmo de analisis sentimental, aqui se puede ver el porque se pone cierto valor a la propiedad PostSentimentalType</rdfs:comment>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#float"/>
    <rdfs:domain rdf:resource="#PostIn"/>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:ID="login">
    <rdfs:domain>
      <swb:Interface rdf:ID="Authenticable">
        <rdfs:subClassOf rdf:resource="http://www.semanticwebbuilder.org/swb4/ontology#SWBInterface"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
      </swb:Interface>
    </rdfs:domain>
    <rdfs:comment>Login o usuario con la cual una organización se puede conectar a una determinada instancia de Red Social.</rdfs:comment>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    <swb:displayProperty>
      <swbxf:DisplayProperty rdf:ID="dp_login">
        <swbxf:formElement rdf:resource="http://www.semanticwebbuilder.org/swb4/xforms/ontology#TextBox"/>
        <swb:index rdf:datatype="http://www.w3.org/2001/XMLSchema#int"
        >1</swb:index>
      </swbxf:DisplayProperty>
    </swb:displayProperty>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:ID="IntensityValue">
    <rdfs:comment>Valor de intensidad de la palabra</rdfs:comment>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#float"/>
    <rdfs:domain>
      <swb:Class rdf:ID="SentimentWords">
        <rdfs:comment>Conjunto de Palabras a buscar dentro de los Post que llegan por el Listener y con las cuales se podra determinar el sentimiento y la intensidad de los mismos(Post).</rdfs:comment>
        <rdfs:subClassOf rdf:resource="http://www.semanticwebbuilder.org/swb4/ontology#SWBClass"/>
        <swb:autogenId rdf:datatype="http://www.w3.org/2001/XMLSchema#boolean"
        >false</swb:autogenId>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
      </swb:Class>
    </rdfs:domain>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:ID="PoolTime">
    <swb:displayProperty>
      <swbxf:DisplayProperty rdf:ID="dp_PoolTime">
        <swbxf:propGroup rdf:resource="#pg_socialConfig"/>
        <swb:index rdf:datatype="http://www.w3.org/2001/XMLSchema#int"
        >20</swb:index>
        <swbxf:formElement rdf:resource="http://www.semanticwebbuilder.org/swb4/xforms/ontology#TextBox"/>
      </swbxf:DisplayProperty>
    </swb:displayProperty>
    <rdfs:label xml:lang="en">Time to search information</rdfs:label>
    <rdfs:label xml:lang="es">Tiempo para Busqueda de Información</rdfs:label>
    <rdfs:comment>Tiempo en que se ira a buscar información a una determinada red social</rdfs:comment>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#int"/>
    <rdfs:domain rdf:resource="#SocialNetwork"/>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:ID="snu_klout">
    <rdfs:domain rdf:resource="#SocialNetworkUser"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#int"/>
    <rdfs:comment>Klout de un usuario en una determinada red social</rdfs:comment>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:ID="filterSentimentalNeutral">
    <rdfs:comment>Propiedad que indica si en el stream se desea aceptar que entren los mensajes que sean clasificados con sentimiento neutro</rdfs:comment>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#boolean"/>
    <rdfs:domain rdf:resource="#Stream"/>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:ID="accessTokenSecret">
    <rdfs:domain>
      <swb:Interface rdf:ID="Oauthable">
        <rdfs:comment>Interface que se le agrega a las clases de tipo SocialNetwork a las que sea necesaria la autenticación mediante OAuth.</rdfs:comment>
        <rdfs:subClassOf rdf:resource="http://www.semanticwebbuilder.org/swb4/ontology#SWBInterface"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
      </swb:Interface>
    </rdfs:domain>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >Almacena un token secreto para realizar publicaciones, por conveniencia no utiliza displayProperty</rdfs:comment>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:ID="postRetweets">
    <rdfs:domain rdf:resource="#PostIn"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#int"/>
    <rdfs:comment>Veces que ha sido re-enviado el post</rdfs:comment>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:ID="nextDatetoSearch">
    <rdfs:domain rdf:resource="#SocialNetwork"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    <rdfs:comment>Fecha siguiente para busqueda en una red social.</rdfs:comment>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:ID="competition">
    <swb:displayProperty>
      <swbxf:DisplayProperty rdf:ID="dp_competition">
        <swbxf:propGroup rdf:resource="#pg_socialConfig"/>
        <swbxf:formElement rdf:resource="http://www.semanticwebbuilder.org/swb4/xforms/ontology#TextBox"/>
        <swb:index rdf:datatype="http://www.w3.org/2001/XMLSchema#int"
        >20</swb:index>
      </swbxf:DisplayProperty>
    </swb:displayProperty>
    <rdfs:label xml:lang="en">Competition</rdfs:label>
    <rdfs:label xml:lang="es">Competencia</rdfs:label>
    <rdfs:comment>Palabras a monitoreas acerca de la competencia</rdfs:comment>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    <rdfs:domain rdf:resource="#WordsToMonitor"/>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:ID="snu_id">
    <rdfs:domain rdf:resource="#SocialNetworkUser"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    <rdfs:comment>Id del usuario en una determinada red social, es decir, en twitter, facebook, google+, youtube, flicker, etc.</rdfs:comment>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:ID="SentimentType">
    <rdfs:domain>
      <swb:Class rdf:ID="SentimentalLearningPhrase">
        <rdfs:subClassOf rdf:resource="http://www.semanticwebbuilder.org/swb4/ontology#SWBClass"/>
        <rdfs:comment>Palabras y Frases en secuencia que puede ir aprendiendo el sistema, esto por medio de la re-clasificación manual de los mensajes por parte de los usuarios</rdfs:comment>
        <swb:autogenId rdf:datatype="http://www.w3.org/2001/XMLSchema#boolean"
