    <rdfs:domain rdf:resource="#Secreteable"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    <swb:displayProperty>
      <swbxf:DisplayProperty rdf:ID="dp_secretkey">
        <swbxf:formElement rdf:resource="http://www.semanticwebbuilder.org/swb4/xforms/ontology#TextBox"/>
      </swbxf:DisplayProperty>
    </swb:displayProperty>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:ID="wpImg">
    <rdfs:domain rdf:resource="#ZulWebPage"/>
    <swb:displayProperty>
      <swbxf:DisplayProperty rdf:ID="dp_wpImage">
        <swbxf:propGroup rdf:resource="#pg_socialConfig"/>
        <swbxf:formElement>
          <swb:DojoImageUpload rdf:ID="socialImageWP">
            <swbxf:imgThumbnail rdf:datatype="http://www.w3.org/2001/XMLSchema#boolean"
