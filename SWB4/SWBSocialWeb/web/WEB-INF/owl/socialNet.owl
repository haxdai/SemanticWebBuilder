        <rdfs:subClassOf rdf:resource="http://www.semanticwebbuilder.org/swb4/ontology#SWBInterface"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
      </swb:Interface>
    </rdfs:subClassOf>
    <rdfs:subClassOf rdf:resource="#Messageable"/>
