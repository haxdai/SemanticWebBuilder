  <owl:DatatypeProperty rdf:ID="st_numDays2ClosePostOuts">
    <rdfs:domain rdf:resource="#SocialTopic"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#int"/>
    <rdfs:comment>Número de días para cerrar el monitoreo de las respuestas de los PostOut del SocialTopic. Maximo 99 días., Si colocan 0, no se monitorearan los PostOut en ese SocialTopic.</rdfs:comment>
    <swb:displayProperty>
      <swbxf:DisplayProperty rdf:ID="dp_st_numDays2ClosePostOuts">
        <swbxf:propInvalidMessage>Value must be between 0 and 99</swbxf:propInvalidMessage>
        <swbxf:formElement>
          <swbxf:Text rdf:ID="st_numDay2closePostOuts_fe">
            <swb:maxLength rdf:datatype="http://www.w3.org/2001/XMLSchema#int"
            >2</swb:maxLength>
            <swbxf:textRegExp rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
            >^([0-9]|[1-9][0-9]|[1-9])$</swbxf:textRegExp>
          </swbxf:Text>
        </swbxf:formElement>
        <swbxf:propInvalidMessage rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
        >Valor debe ser entre 0 y 99</swbxf:propInvalidMessage>
        <swb:index rdf:datatype="http://www.w3.org/2001/XMLSchema#int"
        >120</swb:index>
      </swbxf:DisplayProperty>
    </swb:displayProperty>
    <swb:defaultValue rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >30</swb:defaultValue>
    <rdfs:label xml:lang="es">Días para monitorear mensajes de salida</rdfs:label>
    <rdfs:label xml:lang="en">Days to monitor out messages</rdfs:label>
  </owl:DatatypeProperty>
