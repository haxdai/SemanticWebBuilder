    <rdfs:comment>SocialNetwork a la cual se envia el postOut</rdfs:comment>
    <owl:inverseOf rdf:resource="#haspopr_socialNetworkInv"/>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="#hasSocialNetUserInv">
    <rdfs:comment>Lista todos las instancias de SocialNetworkUser que esten asociados con un determinado Stream, se utiliza para las campañas.</rdfs:comment>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="#hasChildSocialTopicInv">
