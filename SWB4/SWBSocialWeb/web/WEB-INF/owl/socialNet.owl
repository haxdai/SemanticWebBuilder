        <rdfs:subClassOf rdf:resource="#TakeOut"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
      </swb:Class>
    </rdfs:domain>
    <swb:displayProperty>
      <swbxf:DisplayProperty rdf:ID="dp_puntuationSign"/>
    </swb:displayProperty>
    <rdfs:label xml:lang="es">Signo de Puntuación</rdfs:label>
    <rdfs:label xml:lang="en">Puntuation Sign</rdfs:label>
    <swb:required rdf:datatype="http://www.w3.org/2001/XMLSchema#boolean"
    >true</swb:required>
  </owl:DatatypeProperty>
