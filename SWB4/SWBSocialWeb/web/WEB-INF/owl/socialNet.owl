<?xml version="1.0"?>
<rdf:RDF
    xmlns:swbxf="http://www.semanticwebbuilder.org/swb4/xforms/ontology#"
    xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
    xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
    xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
    xmlns:owl="http://www.w3.org/2002/07/owl#"
    xmlns:swb="http://www.semanticwebbuilder.org/swb4/ontology#"
    xmlns:social="http://www.semanticwebbuilder.org/swb4/social#"
  xml:base="http://www.semanticwebbuilder.org/swb4/social">
  <owl:Ontology rdf:about="">
    <owl:imports rdf:resource="http://www.semanticwebbuilder.org/swb4/ontology"/>
    <owl:versionInfo rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >Created with TopBraid Composer</owl:versionInfo>
  </owl:Ontology>
  <owl:Class rdf:ID="PostTextable">
    <rdfs:comment>Interface que contine propiedades necesarias para el manejo de un post que contenga un texto o Mensaje.</rdfs:comment>
    <rdfs:subClassOf>
      <swb:Interface rdf:ID="PostDataable">
        <rdfs:subClassOf rdf:resource="http://www.semanticwebbuilder.org/swb4/ontology#SWBInterface"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
      </swb:Interface>
    </rdfs:subClassOf>
    <rdf:type rdf:resource="http://www.semanticwebbuilder.org/swb4/ontology#Interface"/>
  </owl:Class>
  <owl:Class rdf:ID="SocialNetwork">
    <rdfs:comment>Clase que engloba a las diferentes clases que representan cada una de las redes sociales.</rdfs:comment>
    <rdf:type rdf:resource="http://www.semanticwebbuilder.org/swb4/ontology#Class"/>
    <rdfs:subClassOf rdf:resource="http://www.semanticwebbuilder.org/swb4/ontology#Descriptiveable"/>
    <rdfs:subClassOf>
      <swb:Interface rdf:ID="Secreteable">
        <rdfs:comment>Interface que contiene ciertas propiedades utiles para la autenticación de ciertas redes sociales (ver mas adelante si se unifica con Oauthable)</rdfs:comment>
        <rdfs:subClassOf rdf:resource="http://www.semanticwebbuilder.org/swb4/ontology#SWBInterface"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
      </swb:Interface>
    </rdfs:subClassOf>
    <swb:autogenId rdf:datatype="http://www.w3.org/2001/XMLSchema#boolean"
    >true</swb:autogenId>
    <rdfs:subClassOf>
      <owl:Class rdf:ID="Listenerable"/>
    </rdfs:subClassOf>
    <rdfs:subClassOf rdf:resource="http://www.semanticwebbuilder.org/swb4/ontology#SWBClass"/>
    <rdfs:subClassOf rdf:resource="http://www.semanticwebbuilder.org/swb4/ontology#Tagable"/>
    <swb:displayObject>
      <swbxf:DisplayObject rdf:ID="do_SocialNetwork">
        <swb:iconClass rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
        >swbIconSocialNetwok</swb:iconClass>
      </swbxf:DisplayObject>
    </swb:displayObject>
    <rdfs:subClassOf rdf:resource="http://www.semanticwebbuilder.org/swb4/ontology#Trashable"/>
    <rdfs:label xml:lang="es">Red Social</rdfs:label>
    <rdfs:subClassOf rdf:resource="http://www.semanticwebbuilder.org/swb4/ontology#Activeable"/>
    <rdfs:subClassOf rdf:resource="http://www.semanticwebbuilder.org/swb4/ontology#Traceable"/>
    <rdfs:label xml:lang="en">Social Network</rdfs:label>
  </owl:Class>
  <owl:Class rdf:ID="PostVideoable">
    <rdfs:comment>Interface que contine propiedades necesarias para el manejo de un post que contenga un Video.</rdfs:comment>
    <rdfs:subClassOf rdf:resource="#PostDataable"/>
    <rdf:type rdf:resource="http://www.semanticwebbuilder.org/swb4/ontology#Interface"/>
  </owl:Class>
  <owl:Class rdf:ID="TakeOut">
    <rdfs:subClassOf rdf:resource="http://www.semanticwebbuilder.org/swb4/ontology#SWBClass"/>
    <rdfs:comment>Clase que englobara elementos a ser quitados en los mensajes de las diferentes redes sociales, de tal manera que se puedan comparar cada uno de las palabras de los mensajes con las palabras de valor sentimental que se tengan almacenadas en la clase SentimentalWords.</rdfs:comment>
    <rdf:type rdf:resource="http://www.semanticwebbuilder.org/swb4/ontology#Class"/>
  </owl:Class>
  <owl:Class rdf:ID="DeleteControlable">
    <rdfs:subClassOf rdf:resource="http://www.semanticwebbuilder.org/swb4/ontology#SWBInterface"/>
    <rdfs:comment>Interface que define que las instancias de una clase pueden ser eliminadas</rdfs:comment>
    <rdf:type rdf:resource="http://www.semanticwebbuilder.org/swb4/ontology#Interface"/>
  </owl:Class>
  <owl:Class rdf:ID="Stream">
    <rdfs:subClassOf rdf:resource="http://www.semanticwebbuilder.org/swb4/ontology#FilterableClass"/>
    <rdfs:comment>Clase que contendra los streams que configurados para cada usuario</rdfs:comment>
    <rdfs:label>Stream</rdfs:label>
    <swb:autogenId rdf:datatype="http://www.w3.org/2001/XMLSchema#boolean"
    >true</swb:autogenId>
    <rdfs:subClassOf rdf:resource="http://www.semanticwebbuilder.org/swb4/ontology#Traceable"/>
    <rdfs:subClassOf rdf:resource="http://www.semanticwebbuilder.org/swb4/ontology#Trashable"/>
    <rdfs:subClassOf>
      <swb:Interface rdf:ID="Geolocable">
        <rdfs:comment>Interface que contiene propiedades de localización que seran aplicadas a los mensajes que lleguen desde las redes sociales</rdfs:comment>
        <rdfs:subClassOf rdf:resource="http://www.semanticwebbuilder.org/swb4/ontology#SWBInterface"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
      </swb:Interface>
    </rdfs:subClassOf>
    <rdfs:subClassOf rdf:resource="http://www.semanticwebbuilder.org/swb4/ontology#Activeable"/>
    <rdfs:subClassOf>
      <owl:Class rdf:ID="SocialRuleRefable"/>
    </rdfs:subClassOf>
    <rdfs:subClassOf rdf:resource="http://www.semanticwebbuilder.org/swb4/ontology#Descriptiveable"/>
    <swb:displayObject>
      <swbxf:DisplayObject rdf:ID="do_Stream">
        <swb:iconClass rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
        >swbIconStream</swb:iconClass>
      </swbxf:DisplayObject>
    </swb:displayObject>
    <rdfs:subClassOf rdf:resource="http://www.semanticwebbuilder.org/swb4/ontology#SWBClass"/>
    <rdf:type rdf:resource="http://www.semanticwebbuilder.org/swb4/ontology#Class"/>
  </owl:Class>
  <owl:Class rdf:about="#SocialRuleRefable">
    <rdfs:subClassOf rdf:resource="http://www.semanticwebbuilder.org/swb4/ontology#Referensable"/>
    <rdfs:comment>Interfaz que define propiedades para elementos que pueden referencia a reglas sociales</rdfs:comment>
    <rdf:type rdf:resource="http://www.semanticwebbuilder.org/swb4/ontology#Interface"/>
  </owl:Class>
  <owl:Class rdf:ID="SocialTopic">
    <rdfs:subClassOf rdf:resource="http://www.semanticwebbuilder.org/swb4/ontology#Trashable"/>
    <rdfs:comment>Catalogo de temas de un modelo (Marca)</rdfs:comment>
    <rdfs:subClassOf rdf:resource="http://www.semanticwebbuilder.org/swb4/ontology#UserGroupRefable"/>
    <rdfs:subClassOf rdf:resource="http://www.semanticwebbuilder.org/swb4/ontology#Descriptiveable"/>
    <rdfs:subClassOf rdf:resource="http://www.semanticwebbuilder.org/swb4/ontology#SWBClass"/>
    <rdfs:subClassOf rdf:resource="http://www.semanticwebbuilder.org/swb4/ontology#Tagable"/>
    <swb:autogenId rdf:datatype="http://www.w3.org/2001/XMLSchema#boolean"
    >false</swb:autogenId>
    <rdfs:subClassOf rdf:resource="http://www.semanticwebbuilder.org/swb4/ontology#Activeable"/>
    <rdfs:subClassOf rdf:resource="http://www.semanticwebbuilder.org/swb4/ontology#Traceable"/>
    <rdfs:subClassOf>
      <swb:Interface rdf:ID="SocialPFlowRefable">
        <rdfs:comment>Interfaz que define propiedades para elementos que pueden tener referencia a flujos de publicacion</rdfs:comment>
        <rdfs:subClassOf rdf:resource="http://www.semanticwebbuilder.org/swb4/ontology#Referensable"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
      </swb:Interface>
    </rdfs:subClassOf>
    <swb:displayObject>
      <swbxf:DisplayObject rdf:ID="do_SocialTopic">
        <swbxf:dropMatchLevel rdf:datatype="http://www.w3.org/2001/XMLSchema#int"
        >2</swbxf:dropMatchLevel>
        <swbxf:dragSupport rdf:datatype="http://www.w3.org/2001/XMLSchema#boolean"
        >true</swbxf:dragSupport>
        <swb:iconClass rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
        >swbIconSocialTopic</swb:iconClass>
      </swbxf:DisplayObject>
    </swb:displayObject>
    <rdfs:label xml:lang="en">Topic</rdfs:label>
    <rdf:type rdf:resource="http://www.semanticwebbuilder.org/swb4/ontology#Class"/>
    <rdfs:label xml:lang="es">Tema</rdfs:label>
  </owl:Class>
  <owl:Class rdf:about="#Listenerable">
    <rdfs:subClassOf rdf:resource="http://www.semanticwebbuilder.org/swb4/ontology#SWBInterface"/>
    <rdfs:comment>Esta interfaz se le agrega a las clases de tipo SocialNetwork que se va ha desear que escuchen en las redes sociales, lo cual en teoría deberían de ser a todas, pero por si en un futuro de desea que no sea así.</rdfs:comment>
    <rdf:type rdf:resource="http://www.semanticwebbuilder.org/swb4/ontology#Interface"/>
  </owl:Class>
  <owl:Class rdf:ID="Youtube">
    <swb:displayObject>
      <swbxf:DisplayObject rdf:ID="do_YouTube">
        <swb:iconClass rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
        >swbIconTwitter</swb:iconClass>
      </swbxf:DisplayObject>
    </swb:displayObject>
    <swb:autogenId rdf:datatype="http://www.w3.org/2001/XMLSchema#boolean"
    >true</swb:autogenId>
    <rdfs:label xml:lang="en">YouTube Account</rdfs:label>
    <rdfs:label xml:lang="es">Cuenta de Youtube</rdfs:label>
    <rdfs:subClassOf>
      <swb:Interface rdf:ID="Oauthable">
        <rdfs:comment>Interface que se le agrega a las clases de tipo SocialNetwork a las que sea necesaria la autenticación mediante OAuth.</rdfs:comment>
        <rdfs:subClassOf rdf:resource="http://www.semanticwebbuilder.org/swb4/ontology#SWBInterface"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
      </swb:Interface>
    </rdfs:subClassOf>
    <rdfs:comment>Clase que almacenara las diferentes cuentas de una organización para la red social YouTube.</rdfs:comment>
    <rdf:type rdf:resource="http://www.semanticwebbuilder.org/swb4/ontology#Class"/>
    <rdfs:subClassOf>
      <swb:Interface rdf:ID="DeveloperKeyable">
        <rdfs:comment>Propiedad Developer Key, para redes sociales de Google por el momento (youtube, google+, etc), sin embargo, despues podemos querer integrar alguna otra red social que pueda utilizar este campo.</rdfs:comment>
        <rdfs:subClassOf rdf:resource="http://www.semanticwebbuilder.org/swb4/ontology#SWBInterface"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
      </swb:Interface>
    </rdfs:subClassOf>
    <rdfs:subClassOf rdf:resource="#SocialNetwork"/>
    <rdfs:subClassOf>
      <swb:Interface rdf:ID="Videoable">
        <rdfs:comment>Interface que es utilizada para definir metodos necesarios para el envio y recepción de Post de tipo Video en las diferentes redes sociales que así lo requieran.</rdfs:comment>
        <rdfs:subClassOf>
          <owl:Class rdf:ID="SocialNetPostable"/>
        </rdfs:subClassOf>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
      </swb:Interface>
    </rdfs:subClassOf>
  </owl:Class>
  <owl:Class rdf:about="#SocialNetPostable">
    <rdfs:comment>Interface que agrupa interfaces utiles para manejo de cada uno de los tipos de post que pueden ser enviados y recibidos en las diferentes redes sociales.</rdfs:comment>
    <rdfs:subClassOf rdf:resource="http://www.semanticwebbuilder.org/swb4/ontology#SWBInterface"/>
    <rdf:type rdf:resource="http://www.semanticwebbuilder.org/swb4/ontology#Interface"/>
  </owl:Class>
  <owl:Class rdf:ID="PostOutContainer">
    <rdfs:subClassOf rdf:resource="http://www.semanticwebbuilder.org/swb4/ontology#SWBClass"/>
    <rdfs:subClassOf rdf:resource="http://www.semanticwebbuilder.org/swb4/ontology#Traceable"/>
    <swb:autogenId rdf:datatype="http://www.w3.org/2001/XMLSchema#boolean"
    >true</swb:autogenId>
    <rdfs:comment>Clase a Cambiar despues por "Relacional". Clase que contiene TODOS los post de salida (PostOut) que han sido enviados a una determinada red social. La intención de crear esta clase es para que se agrupen los Post de cada red social por mes y año, y de esta manera sea mucho mas sencillo, optimo y rapido realizar las busquedas. De acuerdo al PostOut, puede buscar si este existe en la clase PostListenerContainerBase, si si existe, quiere decir que es un PostOut que se origino mediante la contestación de un PostIn, si no existe en esa clase, es que es un PostOut que se origino como nuevo, sin que haya sido contestación de un PostIn.</rdfs:comment>
    <rdf:type rdf:resource="http://www.semanticwebbuilder.org/swb4/ontology#Class"/>
  </owl:Class>
  <owl:Class rdf:ID="SocialSite">
    <swb:hasClass>
      <owl:Class rdf:ID="SendPost"/>
    </swb:hasClass>
    <swb:hasHerarquicalNode>
      <swbxf:HerarquicalNode rdf:ID="hn_SocialTopic">
        <swb:iconClass rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
        >swbIconSocialTopicGrp</swb:iconClass>
        <swb:title xml:lang="es">Tema</swb:title>
        <swb:title xml:lang="en">Topic</swb:title>
        <swb:index rdf:datatype="http://www.w3.org/2001/XMLSchema#int"
        >20</swb:index>
        <swbxf:heClass rdf:resource="#SocialTopic"/>
      </swbxf:HerarquicalNode>
    </swb:hasHerarquicalNode>
    <swb:hasHerarquicalNode>
      <swbxf:HerarquicalNode rdf:ID="hn_SocialRules">
        <swb:iconClass rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
        >swbIconRuleGrp</swb:iconClass>
        <swb:index rdf:datatype="http://www.w3.org/2001/XMLSchema#int"
        >40</swb:index>
        <swbxf:heClass>
          <owl:Class rdf:ID="SocialRule"/>
        </swbxf:heClass>
        <swb:title xml:lang="es">Regla Social</swb:title>
        <swb:title xml:lang="en">Social Rule</swb:title>
      </swbxf:HerarquicalNode>
    </swb:hasHerarquicalNode>
    <rdfs:label xml:lang="es">Sitio Social</rdfs:label>
    <swb:hasClass>
      <swb:Class rdf:ID="SocialPFlowRef">
        <rdfs:comment>Referencia a un objeto de tipo PFlow</rdfs:comment>
        <swb:autogenId rdf:datatype="http://www.w3.org/2001/XMLSchema#boolean"
        >true</swb:autogenId>
        <rdfs:subClassOf>
          <swb:Interface rdf:ID="SocialPFlowable">
            <rdfs:comment>Interfaz que define propiedades para elementos que pueden asociar un flujo de publicacion</rdfs:comment>
            <rdfs:subClassOf rdf:resource="http://www.semanticwebbuilder.org/swb4/ontology#SWBInterface"/>
            <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
          </swb:Interface>
        </rdfs:subClassOf>
        <rdfs:subClassOf rdf:resource="http://www.semanticwebbuilder.org/swb4/ontology#Reference"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
      </swb:Class>
    </swb:hasClass>
    <swb:hasClass rdf:resource="#SocialTopic"/>
    <swb:displayObject>
      <swbxf:DisplayObject rdf:ID="do_SocialSite">
        <swb:iconClass rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
        >swbIconSocialSite</swb:iconClass>
      </swbxf:DisplayObject>
    </swb:displayObject>
    <swb:hasClass>
      <swb:Class rdf:ID="MarkMsgAsPrioritary">
        <swb:autogenId rdf:datatype="http://www.w3.org/2001/XMLSchema#boolean"
        >true</swb:autogenId>
        <rdfs:comment>Acción específica mediante la cual se marca un mensaje como prioritario. Esto en la propiedad "IsPrioritary" de un mensaje (Post).</rdfs:comment>
        <rdfs:label xml:lang="es">Marcar un mensaje como prioritario</rdfs:label>
        <swb:displayObject>
          <swbxf:DisplayObject rdf:ID="do_markAsPriorityAct">
            <swb:iconClass rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
            >swbIconActionPrioritary</swb:iconClass>
          </swbxf:DisplayObject>
        </swb:displayObject>
        <rdfs:subClassOf>
          <swb:Class rdf:ID="Action">
            <rdfs:subClassOf rdf:resource="http://www.semanticwebbuilder.org/swb4/ontology#SWBClass"/>
            <rdfs:comment>Clase padre de todas las acciones posibles en swbsocial</rdfs:comment>
            <rdfs:label xml:lang="en">Action</rdfs:label>
            <swb:autogenId rdf:datatype="http://www.w3.org/2001/XMLSchema#boolean"
            >true</swb:autogenId>
            <rdfs:subClassOf rdf:resource="http://www.semanticwebbuilder.org/swb4/ontology#Traceable"/>
            <rdfs:label xml:lang="es">Acción</rdfs:label>
            <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
            <swb:displayObject>
              <swbxf:DisplayObject rdf:ID="do_socialAction">
                <swb:iconClass rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
                >swbIconRule</swb:iconClass>
                <swbxf:dragSupport rdf:datatype="http://www.w3.org/2001/XMLSchema#boolean"
                >true</swbxf:dragSupport>
                <swbxf:dropMatchLevel rdf:datatype="http://www.w3.org/2001/XMLSchema#int"
                >2</swbxf:dropMatchLevel>
                <rdfs:label xml:lang="es">Acción</rdfs:label>
                <rdfs:label xml:lang="en">Action</rdfs:label>
                <swbxf:doNotInstanceable rdf:datatype="http://www.w3.org/2001/XMLSchema#boolean"
                >true</swbxf:doNotInstanceable>
              </swbxf:DisplayObject>
            </swb:displayObject>
            <rdfs:subClassOf rdf:resource="http://www.semanticwebbuilder.org/swb4/ontology#Descriptiveable"/>
          </swb:Class>
        </rdfs:subClassOf>
        <rdfs:label xml:lang="en">Action of marking a message as prioritary</rdfs:label>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
      </swb:Class>
    </swb:hasClass>
    <swb:hasHerarquicalNode>
      <swbxf:HerarquicalNode rdf:ID="hn_SocialNets">
        <swb:iconClass rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
        >swbIconSocialNetGrp</swb:iconClass>
        <swb:title xml:lang="es">Red Social</swb:title>
        <swb:title xml:lang="en">Social Network</swb:title>
        <swb:hasHerarquicalNode>
          <swbxf:HerarquicalNode rdf:ID="hn_Twitter">
            <swb:iconClass rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
            >swbIconTwitterGrp</swb:iconClass>
            <swb:index rdf:datatype="http://www.w3.org/2001/XMLSchema#int"
            >20</swb:index>
            <swb:title rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
            >Twitter</swb:title>
            <swbxf:heClass>
              <swb:Class rdf:ID="Twitter">
                <rdfs:subClassOf rdf:resource="#SocialNetwork"/>
                <rdfs:comment>Clase que almacenara las diferentes cuentas de una organización para la red social Twitter.</rdfs:comment>
                <rdfs:subClassOf>
                  <swb:Interface rdf:ID="Messageable">
                    <rdfs:comment>Interface que es utilizada para definir metodos necesarios para el envio y recepción de Post de tipo Mensaje en las diferentes redes sociales que así lo requieran.</rdfs:comment>
                    <rdfs:subClassOf rdf:resource="#SocialNetPostable"/>
                    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
                  </swb:Interface>
                </rdfs:subClassOf>
                <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
                <rdfs:label xml:lang="es">Cuenta de Twitter</rdfs:label>
                <rdfs:subClassOf>
                  <swb:Interface rdf:ID="Photoable">
                    <rdfs:comment>Interface que es utilizada para definir metodos necesarios para el envio y recepción de Post de tipo Foto en las diferentes redes sociales que así lo requieran.</rdfs:comment>
                    <rdfs:subClassOf rdf:resource="#SocialNetPostable"/>
                    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
                  </swb:Interface>
                </rdfs:subClassOf>
                <swb:autogenId rdf:datatype="http://www.w3.org/2001/XMLSchema#boolean"
                >true</swb:autogenId>
                <rdfs:label xml:lang="en">Twitter Account</rdfs:label>
                <rdfs:subClassOf>
                  <owl:Class rdf:ID="Kloutable"/>
                </rdfs:subClassOf>
                <rdfs:subClassOf rdf:resource="#Oauthable"/>
                <swb:displayObject>
                  <swbxf:DisplayObject rdf:ID="do_Twitter">
                    <swb:iconClass rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
                    >swbIconTwitter</swb:iconClass>
                  </swbxf:DisplayObject>
                </swb:displayObject>
              </swb:Class>
            </swbxf:heClass>
          </swbxf:HerarquicalNode>
        </swb:hasHerarquicalNode>
        <swb:index rdf:datatype="http://www.w3.org/2001/XMLSchema#int"
        >10</swb:index>
        <swb:hasHerarquicalNode>
          <swbxf:HerarquicalNode rdf:ID="hn_Facebook">
            <swb:iconClass rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
            >swbIconFacebookGrp</swb:iconClass>
            <swb:index rdf:datatype="http://www.w3.org/2001/XMLSchema#int"
            >20</swb:index>
            <swb:title rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
            >Facebook</swb:title>
            <swbxf:heClass>
              <swb:Class rdf:ID="Facebook">
                <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
                <swb:displayObject>
                  <swbxf:DisplayObject rdf:ID="do_Facebook">
                    <swb:iconClass rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
                    >swbIconFacebook</swb:iconClass>
                  </swbxf:DisplayObject>
                </swb:displayObject>
                <rdfs:subClassOf rdf:resource="#Messageable"/>
                <rdfs:label xml:lang="es">Cuenta de Facebook</rdfs:label>
                <rdfs:subClassOf rdf:resource="#Photoable"/>
