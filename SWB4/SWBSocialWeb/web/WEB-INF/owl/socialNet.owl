    <rdfs:range rdf:resource="#SocialPFlow"/>
    <owl:inverseOf rdf:resource="#hasSocialPFlowRefInv"/>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:ID="popr_socialNetwork">
    <rdfs:domain rdf:resource="#PostOutPrivacyRelation"/>
