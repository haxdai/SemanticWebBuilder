    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#float"/>
    <rdfs:domain rdf:resource="#Geolocable"/>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:ID="secretKey">
    <rdfs:comment>Propiedad que almacena el SecretKey de una determinada Red Social que requiera de este para su conección.</rdfs:comment>
