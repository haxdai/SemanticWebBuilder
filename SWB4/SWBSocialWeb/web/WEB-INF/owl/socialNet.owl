            <swbxf:imgCrop rdf:datatype="http://www.w3.org/2001/XMLSchema#boolean"
            >false</swbxf:imgCrop>
            <swbxf:fileMaxSize rdf:datatype="http://www.w3.org/2001/XMLSchema#long"
            >50000</swbxf:fileMaxSize>
            <swbxf:fileFilter rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
            > jpg file:*.jpg|jpeg file:*.jpeg|png file:*.png</swbxf:fileFilter>
          </swb:DojoImageUpload>
        </swbxf:formElement>
      </swbxf:DisplayProperty>
    </swb:displayProperty>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    <rdfs:comment>Imagen - Icono de una sección, esta se desplegara en el menú de SWBSocial</rdfs:comment>
    <rdfs:label xml:lang="es">Imagen</rdfs:label>
    <rdfs:label xml:lang="en">Image</rdfs:label>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:ID="geoDistanceUnit">
    <rdfs:comment>Unidad de medida para la distancia entre el centro y el radio. Ej. KM, MI</rdfs:comment>
    <swb:displayProperty>
      <swbxf:DisplayProperty rdf:ID="dp_geoDistanceUnit"/>
    </swb:displayProperty>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    <rdfs:domain rdf:resource="#Geolocable"/>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:ID="appKey">
    <rdfs:comment>Propiedad que almacena el Aplication Key de una determinada Red Social que requiera de este para su conección.</rdfs:comment>
    <rdfs:label xml:lang="en">Aplication Key</rdfs:label>
    <rdfs:label xml:lang="es">Aplicación</rdfs:label>
    <rdfs:domain rdf:resource="#Secreteable"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    <swb:displayProperty>
      <swbxf:DisplayProperty rdf:ID="dp_appKey">
        <swbxf:formElement rdf:resource="http://www.semanticwebbuilder.org/swb4/xforms/ontology#TextBox"/>
      </swbxf:DisplayProperty>
    </swb:displayProperty>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:ID="stream_resultPagnum">
    <rdfs:comment>Número de resultados que se despliegan en las páginas de los reportes</rdfs:comment>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#int"/>
    <rdfs:domain rdf:resource="#Stream"/>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:ID="plc_month">
    <rdfs:comment>Mes en el cual es creada una instancia de PostListenerContainer,esta propiedad servira mucho  para buscar los postIn de manera optima, ya que pudieran ser muchos en un corto tiempo.</rdfs:comment>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    <rdfs:domain rdf:resource="#PostListenerContainer"/>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:ID="sn_authenticated">
    <rdfs:domain rdf:resource="#SocialNetwork"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#boolean"/>
    <swb:displayProperty>
      <swbxf:DisplayProperty rdf:ID="dp_sn_authenticated">
        <swbxf:propGroup rdf:resource="#pg_socialConfig"/>
      </swbxf:DisplayProperty>
    </swb:displayProperty>
    <rdfs:comment>Bandera que indica si la red social se encuentra antenticada o no, esta propiedad se maneja desde el sistema. Si la red social regresa que la fecha de expiración de la autenticación ha concluido, se pone en false, para que despues se autentique nuevamente la cuenta de manera manual desde el modulo de cuentas de redes sociales.</rdfs:comment>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:ID="stream_phrase">
    <rdfs:comment>Frase a monitorear en un determinado stream, cada stream tiene sus propias frasea a monitorear.</rdfs:comment>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    <rdfs:domain rdf:resource="#Stream"/>
    <swb:displayProperty>
      <swbxf:DisplayProperty rdf:ID="dp_stream_phase">
        <swbxf:propGroup rdf:resource="#pg_socialConfig"/>
        <swbxf:formElement rdf:resource="http://www.semanticwebbuilder.org/swb4/xforms/ontology#TextArea_Description"/>
        <swb:index rdf:datatype="http://www.w3.org/2001/XMLSchema#int"
        >10</swb:index>
      </swbxf:DisplayProperty>
    </swb:displayProperty>
    <swb:propertyCodeName rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >phrase</swb:propertyCodeName>
    <rdfs:label xml:lang="es">Frase</rdfs:label>
    <rdfs:label xml:lang="en">Phrase</rdfs:label>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:ID="snu_name">
    <rdfs:domain rdf:resource="#SocialNetworkUser"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    <rdfs:comment>Nombre del usuario en la Red Social</rdfs:comment>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:ID="accessToken">
    <rdfs:domain rdf:resource="#Oauthable"/>
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >Almacena el access Token autorizado para hacer publicaciones, por conveniencia no utiliza displayProperty</rdfs:comment>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:ID="zulResourcePath">
    <swb:displayProperty>
      <swbxf:DisplayProperty rdf:ID="dp_zulResourcePath">
        <swbxf:propGroup rdf:resource="#pg_socialConfig"/>
        <swbxf:formElement rdf:resource="http://www.semanticwebbuilder.org/swb4/xforms/ontology#TextBox"/>
        <swb:index rdf:datatype="http://www.w3.org/2001/XMLSchema#int"
        >10</swb:index>
      </swbxf:DisplayProperty>
    </swb:displayProperty>
    <rdfs:comment>Nombre del archivo zul que va ha mostrar un contenido para la página</rdfs:comment>
    <rdfs:label xml:lang="en">Zul File</rdfs:label>
    <rdfs:label xml:lang="es">Archivo Zul</rdfs:label>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    <rdfs:domain rdf:resource="#ZulWebPage"/>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:ID="tokenExpirationDate">
    <rdfs:domain rdf:resource="#Oauthable"/>
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >Almacena la fecha de expiración del token autorizado para hacer publicaciones, por conveniencia no utiliza displayProperty</rdfs:comment>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#dateTime"/>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:ID="preposition">
    <rdfs:comment>Preposición</rdfs:comment>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    <rdfs:domain>
      <swb:Class rdf:ID="Prepositions">
        <swb:autogenId rdf:datatype="http://www.w3.org/2001/XMLSchema#boolean"
        >false</swb:autogenId>
        <rdfs:comment>Preposiciones (De lugar, de tiempo, etc) que no seran tomadas como palabras dentro de los mensajes de los post dentro del listener. Es decir, estas no deben ser evaluadas para analisis sentimental.</rdfs:comment>
        <rdfs:subClassOf rdf:resource="#TakeOut"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
      </swb:Class>
    </rdfs:domain>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:ID="countryCodetoSearch">
    <rdfs:domain rdf:resource="#SocialNetwork"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    <rdfs:comment>Código de pais que servira como filtro, es decir, solo se guardaran los mensajes que se hayan generado en el pais que pertenesca al mismo código de pais que se capture en esta propiedad, esto podría ser para cada instancia (cuenta) de cada red social.</rdfs:comment>
    <swb:displayProperty>
      <swbxf:DisplayProperty rdf:ID="dp_countryCodetoSearch">
        <swbxf:propGroup rdf:resource="#pg_socialConfig"/>
        <swbxf:formElement rdf:resource="http://www.semanticwebbuilder.org/swb4/xforms/ontology#TextBox"/>
      </swbxf:DisplayProperty>
    </swb:displayProperty>
    <rdfs:label xml:lang="es">Código de País a reducir la busqueda</rdfs:label>
    <rdfs:label xml:lang="en">Country Code to reduce the search</rdfs:label>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:ID="geoRadio">
    <swb:displayProperty>
      <swbxf:DisplayProperty rdf:ID="dp_geoRadio"/>
    </swb:displayProperty>
    <rdfs:comment>Distancia del centro a buscar mensajes</rdfs:comment>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#float"/>
    <rdfs:domain rdf:resource="#Geolocable"/>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:ID="filterIntensityHigh">
    <rdfs:domain rdf:resource="#Stream"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#boolean"/>
    <rdfs:comment>Propiedad que indica si en el stream se desea aceptar que entren los mensajes que sean clasificados con intensidad alta</rdfs:comment>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:ID="geoCenterLongitude">
    <swb:displayProperty rdf:resource="#dp_geoCenterLatitude"/>
    <rdfs:comment>Longitud del punto central a buscar mensajes</rdfs:comment>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#float"/>
    <rdfs:domain rdf:resource="#Geolocable"/>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:ID="msg_Text">
    <rdfs:comment>Propiedad que almacena un texto o mensaje en un objeto Post que lo requiera.</rdfs:comment>
    <rdfs:domain rdf:resource="#PostTextable"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    <swb:displayProperty>
      <swbxf:DisplayProperty rdf:ID="dp_msg_Text">
        <swbxf:formElement rdf:resource="http://www.semanticwebbuilder.org/swb4/xforms/ontology#TextArea_Description"/>
      </swbxf:DisplayProperty>
    </swb:displayProperty>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:ID="video">
    <rdfs:comment>Propiedad que almacena el nombre(referencia) de un video en un objeto Post que lo requiera.</rdfs:comment>
    <rdfs:domain rdf:resource="#PostVideoable"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    <swb:displayProperty>
      <swbxf:DisplayProperty rdf:ID="dp_video">
        <swbxf:formElement>
          <swb:DojoFileUpload rdf:ID="Social_Video_Upload">
            <swbxf:fileMaxSize rdf:datatype="http://www.w3.org/2001/XMLSchema#long"
            >200000000</swbxf:fileMaxSize>
            <swbxf:fileFilter rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
            >*.mov:mov|*.MOV:MOV|*.png:PNG</swbxf:fileFilter>
          </swb:DojoFileUpload>
        </swbxf:formElement>
      </swbxf:DisplayProperty>
    </swb:displayProperty>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:ID="postSentimentalType">
    <rdfs:domain rdf:resource="#PostIn"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#int"/>
    <rdfs:comment>Propiedad con valor entero que representa el tipo de Sentimientos que expresa el Post, estos se estan definiendo de esta manera: 0) Neutro 1) Positivo 2)Negativo, estos valores pueden ser mas y permanecer en un objeto tipo colección en lo futuro.</rdfs:comment>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:ID="postSentimentalEmoticonType">
    <rdfs:domain rdf:resource="#PostIn"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#int"/>
    <rdfs:comment>Sentimiento de el mensaje (Post) basado en emoticones encontrados. (1=Positivo;2=Negativo;0=Neutro)</rdfs:comment>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:ID="plc_year">
    <rdfs:comment>Año en el cual es creada una instancia de PostListenerContainer, esta propiedad servira mucho  para buscar los postIn de manera optima, ya que pudieran ser muchos en un corto tiempo.</rdfs:comment>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    <rdfs:domain rdf:resource="#PostListenerContainer"/>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:ID="friends">
    <rdfs:domain rdf:resource="#SocialNetworkUser"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#int"/>
    <rdfs:comment>Número de usuarios a los que yo sigo (así se maneja en twitter), en facebook aqui pondría a mis amigos y la de followers (que son los que me siguen) según yo no la utilizaría (en facebook)</rdfs:comment>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:ID="facebookUserId">
    <rdfs:comment>Eliminar despues esta propiedad, ver si Jose puede eliminar la propiedad login que se encuentra en la clase padre(SocialNetwork).</rdfs:comment>
    <swb:displayProperty>
      <swbxf:DisplayProperty rdf:ID="dp_facebookUserId">
        <swbxf:formElement rdf:resource="http://www.semanticwebbuilder.org/swb4/xforms/ontology#TextBox"/>
      </swbxf:DisplayProperty>
    </swb:displayProperty>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    <rdfs:domain>
      <swb:Class rdf:ID="Facebook">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
        <swb:displayObject>
          <swbxf:DisplayObject rdf:ID="do_Facebook">
            <swb:iconClass rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
            >facebook.png</swb:iconClass>
            <swbxf:doDispatcher rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
            >x.zul</swbxf:doDispatcher>
          </swbxf:DisplayObject>
        </swb:displayObject>
        <rdfs:subClassOf>
          <swb:Interface rdf:ID="Messageable">
            <rdfs:comment>Interface que es utilizada para definir metodos necesarios para el envio y recepción de Post de tipo Mensaje en las diferentes redes sociales que así lo requieran.</rdfs:comment>
            <rdfs:subClassOf rdf:resource="#SocialNetPostable"/>
            <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
          </swb:Interface>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
          <swb:Interface rdf:ID="Photoable">
            <rdfs:comment>Interface que es utilizada para definir metodos necesarios para el envio y recepción de Post de tipo Foto en las diferentes redes sociales que así lo requieran.</rdfs:comment>
            <rdfs:subClassOf rdf:resource="#SocialNetPostable"/>
            <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
          </swb:Interface>
        </rdfs:subClassOf>
        <rdfs:subClassOf rdf:resource="#Oauthable"/>
        <rdfs:subClassOf rdf:resource="#SocialNetwork"/>
        <rdfs:comment>Clase que almacenara las diferentes cuentas de una organización para la red social Facebook.</rdfs:comment>
        <rdfs:subClassOf>
          <swb:Interface rdf:ID="Videoable">
            <rdfs:comment>Interface que es utilizada para definir metodos necesarios para el envio y recepción de Post de tipo Video en las diferentes redes sociales que así lo requieran.</rdfs:comment>
            <rdfs:subClassOf rdf:resource="#SocialNetPostable"/>
            <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
          </swb:Interface>
        </rdfs:subClassOf>
        <swb:autogenId rdf:datatype="http://www.w3.org/2001/XMLSchema#boolean"
        >true</swb:autogenId>
      </swb:Class>
    </rdfs:domain>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:ID="password">
    <rdfs:domain rdf:resource="#Authenticable"/>
    <rdfs:comment>Password o contraseña con la cual una organización se puede conectar a una determinada instancia de Red Social.</rdfs:comment>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    <swb:displayProperty>
      <swbxf:DisplayProperty rdf:ID="dp_password">
        <swbxf:formElement rdf:resource="http://www.semanticwebbuilder.org/swb4/xforms/ontology#TextBox"/>
        <swb:index rdf:datatype="http://www.w3.org/2001/XMLSchema#int"
        >2</swb:index>
      </swbxf:DisplayProperty>
    </swb:displayProperty>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:ID="month">
    <rdfs:comment>Mes en el cual es creada una instancia de PostContainer,esta propiedad servira mucho  para buscar los postOut de manera optima, ya que pudieran ser muchos en un corto tiempo.</rdfs:comment>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    <rdfs:domain rdf:resource="#PostContainer"/>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:ID="SentimentalValue">
    <rdfs:comment>Valor sentimental de la palabra</rdfs:comment>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#float"/>
    <rdfs:domain rdf:resource="#SentimentWords"/>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:ID="filterIntensityMedium">
    <rdfs:domain rdf:resource="#Stream"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#boolean"/>
    <rdfs:comment>Propiedad que indica si en el stream se desea aceptar que entren los mensajes que sean clasificados con intensidad media</rdfs:comment>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:ID="stream_KloutValue">
    <rdfs:comment>Valor númerico minimo de klout que se desea filtrar para un stream. Los usuarios que tengan este klout o más y que hablen en las redes sociales configuradas para el stream, seran tomados sus mensajes para ser guardados en el sistema.</rdfs:comment>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#int"/>
    <rdfs:domain rdf:resource="#Stream"/>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:ID="year">
    <rdfs:comment>Año en el cual es creada una instancia de PostContainer, esta propiedad servira mucho  para buscar los postOut de manera optima, ya que pudieran ser muchos en un corto tiempo.</rdfs:comment>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    <rdfs:domain rdf:resource="#PostContainer"/>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:ID="puntuationSign">
    <rdfs:comment>Signo de Puntuación</rdfs:comment>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    <rdfs:domain>
      <swb:Class rdf:ID="PunctuationSign">
        <rdfs:comment>Signos de puntuación que seran eliminados de los mensajes de los post dentro del listener. Es decir, estas no deben ser evaluadas para analisis sentimental.</rdfs:comment>
        <swb:autogenId rdf:datatype="http://www.w3.org/2001/XMLSchema#boolean"
        >true</swb:autogenId>
        <rdfs:subClassOf rdf:resource="#TakeOut"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
      </swb:Class>
    </rdfs:domain>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:ID="photo">
    <rdfs:comment>Propiedad que almacena el nombre(referencia) de la foto en un objeto Post que lo requiera.</rdfs:comment>
    <rdfs:domain rdf:resource="#PostImageable"/>
    <swb:displayProperty>
      <swbxf:DisplayProperty rdf:ID="dp_photo">
        <swbxf:formElement>
          <swb:DojoImageUpload rdf:ID="socialDojoImageUpload">
            <swbxf:fileFilter rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
            >*.jpg:JPG|*.gif:GIF|*.png:PNG|*.tiff:TIFF</swbxf:fileFilter>
            <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
            >Cargador de imágenes para social</rdfs:comment>
          </swb:DojoImageUpload>
        </swbxf:formElement>
      </swbxf:DisplayProperty>
    </swb:displayProperty>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:ID="tree_action">
    <rdfs:domain rdf:resource="#TreeNodePage"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    <rdfs:comment>Acción a implementar en un zul para esta opción en el árbol. Ej. "add", "edit", "listen", "opcionx", etc.</rdfs:comment>
    <rdfs:label xml:lang="en">Action</rdfs:label>
    <rdfs:label xml:lang="es">Acción</rdfs:label>
    <swb:propertyCodeName rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >action</swb:propertyCodeName>
    <swb:displayProperty>
      <swbxf:DisplayProperty rdf:ID="dp_tree_action">
        <swb:index rdf:datatype="http://www.w3.org/2001/XMLSchema#int"
        >50</swb:index>
        <swbxf:formElement rdf:resource="http://www.semanticwebbuilder.org/swb4/xforms/ontology#TextBox"/>
        <swbxf:propGroup rdf:resource="#pg_socialConfig"/>
      </swbxf:DisplayProperty>
    </swb:displayProperty>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:ID="wphOverImg">
    <rdfs:label xml:lang="en">hOver Image</rdfs:label>
    <rdfs:label xml:lang="es">Imagen hOver</rdfs:label>
    <swb:displayProperty rdf:resource="#dp_wpImage"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    <rdfs:domain rdf:resource="#ZulWebPage"/>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:ID="stream_PoolTime">
    <swb:propertyCodeName rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >poolTime</swb:propertyCodeName>
    <rdfs:label xml:lang="en">Time to search information</rdfs:label>
    <rdfs:label xml:lang="es">Tiempo para Busqueda de Información</rdfs:label>
    <swb:displayProperty>
      <swbxf:DisplayProperty rdf:ID="dp_stream_PoolTime">
        <swbxf:formElement rdf:resource="http://www.semanticwebbuilder.org/swb4/xforms/ontology#TextBox"/>
        <swbxf:propGroup rdf:resource="#pg_socialConfig"/>
        <swb:index rdf:datatype="http://www.w3.org/2001/XMLSchema#int"
        >60</swb:index>
      </swbxf:DisplayProperty>
    </swb:displayProperty>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#int"/>
    <rdfs:comment>Lapso de tiempo en que se busca la información. Ej. Cada x tiempo</rdfs:comment>
    <rdfs:domain rdf:resource="#Stream"/>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:ID="followers">
    <rdfs:comment>Cantidad de seguidores que tiene el usuario que es almacenado en c/instancia de esta clase (en su propiedad id)</rdfs:comment>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#int"/>
    <rdfs:domain rdf:resource="#SocialNetworkUser"/>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:ID="IntensityType">
    <rdfs:domain rdf:resource="#SentimentalLearningPhrase"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#int"/>
    <rdfs:comment>Valor que le dan los usuarios al clasificar una palabra o secuencia de palabra. 1=Alto;2=Bajo;0=Medio</rdfs:comment>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:ID="Phrase">
    <rdfs:domain rdf:resource="#SentimentalLearningPhrase"/>
    <rdfs:comment>Frase a ser re-clasificada por los usuarios.</rdfs:comment>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:ID="tree_classUri">
    <rdfs:domain rdf:resource="#TreeNodePage"/>
    <swb:propertyCodeName rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >classUri</swb:propertyCodeName>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    <swb:displayProperty>
      <swbxf:DisplayProperty rdf:ID="dp_tree_classUri">
        <swb:index rdf:datatype="http://www.w3.org/2001/XMLSchema#int"
        >5</swb:index>
        <swbxf:formElement rdf:resource="http://www.semanticwebbuilder.org/swb4/xforms/ontology#TextBox"/>
        <swbxf:propGroup rdf:resource="#pg_socialConfig"/>
      </swbxf:DisplayProperty>
    </swb:displayProperty>
    <rdfs:comment>Uri de la clase a ser desplegada en el árbol de navegación como un nodo de tipo Categoría.</rdfs:comment>
    <rdfs:label xml:lang="es">Uri de clase semantica</rdfs:label>
    <rdfs:label xml:lang="en">Semantic Class Uri</rdfs:label>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:ID="isKeepingConnection">
    <swb:displayProperty>
      <swbxf:DisplayProperty rdf:ID="dp_isKeepingConnection">
        <swbxf:propGroup rdf:resource="#pg_socialConfig"/>
        <swb:index rdf:datatype="http://www.w3.org/2001/XMLSchema#int"
        >10</swb:index>
      </swbxf:DisplayProperty>
    </swb:displayProperty>
    <rdfs:comment>Propiedad en la que se puede definir si la red social va ha mantener la conección abierta (mediante Stream Api) o no lo va ha ser (Poolean de información por el listener c/x tiempo),  por defecto es false (Es decir sera por pooleo).</rdfs:comment>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#boolean"/>
    <rdfs:domain>
      <swb:Interface rdf:ID="KeepAliveListenerable">
        <rdfs:comment>Interface para las redes sociales que pueden mantener abierta la conección para realizar las busquedas (listener) como es el caso de Twitter con su Streaming Api.</rdfs:comment>
        <rdfs:subClassOf rdf:resource="http://www.semanticwebbuilder.org/swb4/ontology#SWBInterface"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
      </swb:Interface>
    </rdfs:domain>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:ID="postPlace">
    <rdfs:domain rdf:resource="#PostIn"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    <rdfs:comment>Lugar Geográfico de donde se envía el Post</rdfs:comment>
  </owl:DatatypeProperty>
  <swb:Class rdf:ID="Video">
    <rdfs:subClassOf rdf:resource="#PostVideoable"/>
    <rdfs:comment>Clase que sera creada cuando un post sea de tipo video, como lo es para youtube y facebook (cuando se envía un video a facebook).</rdfs:comment>
    <rdfs:subClassOf rdf:resource="#PostOut"/>
    <swb:autogenId rdf:datatype="http://www.w3.org/2001/XMLSchema#boolean"
    >true</swb:autogenId>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
  </swb:Class>
  <swbxf:DisplayObject rdf:ID="do_Flicker"/>
  <swbxf:DisplayObject rdf:ID="do_Youtube"/>
  <swb:Class rdf:ID="MessageIn">
    <rdfs:subClassOf rdf:resource="#PostTextable"/>
    <rdfs:comment>Clase en donde se almacenan todos los post de tipo Mensaje y que entran por el Listener</rdfs:comment>
    <rdfs:subClassOf rdf:resource="#PostIn"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
  </swb:Class>
  <swb:Class rdf:ID="Twitter">
    <rdfs:subClassOf rdf:resource="#SocialNetwork"/>
    <rdfs:comment>Clase que almacenara las diferentes cuentas de una organización para la red social Twitter.</rdfs:comment>
    <rdfs:subClassOf rdf:resource="#Messageable"/>
    <rdfs:subClassOf rdf:resource="#KeepAliveListenerable"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <rdfs:subClassOf rdf:resource="#Photoable"/>
    <swb:autogenId rdf:datatype="http://www.w3.org/2001/XMLSchema#boolean"
    >true</swb:autogenId>
    <rdfs:subClassOf rdf:resource="#Kloutable"/>
    <rdfs:subClassOf rdf:resource="#Oauthable"/>
    <swb:displayObject>
      <swbxf:DisplayObject rdf:ID="do_Twitter">
        <swb:iconClass rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
        >twitter.png</swb:iconClass>
      </swbxf:DisplayObject>
    </swb:displayObject>
  </swb:Class>
  <swb:CodePackage rdf:ID="pkg">
    <swb:package rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >org.semanticwb.social</swb:package>
    <swb:prefix rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >social</swb:prefix>
  </swb:CodePackage>
  <swb:Class rdf:ID="VideoIn">
    <rdfs:comment>Clase en donde se almacenan todos los post de tipo Video y que entran por el Listener</rdfs:comment>
    <rdfs:subClassOf rdf:resource="#MessageIn"/>
    <rdfs:subClassOf rdf:resource="#PostVideoable"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
  </swb:Class>
  <swb:Class rdf:ID="Message">
    <rdfs:subClassOf rdf:resource="#PostTextable"/>
    <rdfs:comment>Clase que sera creada cuando un post sea de tipo mensaje</rdfs:comment>
    <swb:autogenId rdf:datatype="http://www.w3.org/2001/XMLSchema#boolean"
    >true</swb:autogenId>
    <rdfs:subClassOf rdf:resource="#PostOut"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
  </swb:Class>
  <swbxf:DisplayProperty rdf:ID="dp_removeFromController">
    <swbxf:propGroup rdf:resource="#pg_socialConfig"/>
    <swb:index rdf:datatype="http://www.w3.org/2001/XMLSchema#int"
    >1</swb:index>
  </swbxf:DisplayProperty>
  <swb:Class rdf:ID="Flicker">
    <rdfs:subClassOf rdf:resource="#Oauthable"/>
    <rdfs:subClassOf rdf:resource="#Photoable"/>
    <rdfs:subClassOf rdf:resource="#SocialNetwork"/>
    <swb:autogenId rdf:datatype="http://www.w3.org/2001/XMLSchema#boolean"
    >true</swb:autogenId>
    <rdfs:comment>Clase que almacenara las diferentes cuentas de una organización para la red social Flicker.</rdfs:comment>
    <swb:displayObject rdf:resource="#do_Flicker"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
  </swb:Class>
  <swbxf:DisplayProperty rdf:ID="dp_geoCenterLongitude"/>
  <swb:Class rdf:ID="PhotoIn">
    <rdfs:comment>Clase en donde se almacenan todos los post de tipo Photo y que entran por el Listener</rdfs:comment>
    <rdfs:subClassOf rdf:resource="#MessageIn"/>
    <rdfs:subClassOf rdf:resource="#PostImageable"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
  </swb:Class>
  <swb:Class rdf:ID="Photo">
    <rdfs:subClassOf rdf:resource="#PostImageable"/>
    <rdfs:subClassOf rdf:resource="#PostOut"/>
    <rdfs:comment>Clase que sera creada cuando un post sea de tipo Foto (una imagen), como lo es flicker y facebook (cuando se envía una foto a facebook).</rdfs:comment>
    <swb:autogenId rdf:datatype="http://www.w3.org/2001/XMLSchema#boolean"
    >true</swb:autogenId>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
  </swb:Class>
  <swb:Class rdf:ID="Youtube">
    <rdfs:subClassOf rdf:resource="#Authenticable"/>
    <swb:autogenId rdf:datatype="http://www.w3.org/2001/XMLSchema#boolean"
    >true</swb:autogenId>
    <rdfs:subClassOf rdf:resource="#Videoable"/>
    <rdfs:subClassOf rdf:resource="#SocialNetwork"/>
    <rdfs:comment>Clase que almacenara las diferentes cuentas de una organización para la red social YouTube.</rdfs:comment>
    <swb:displayObject rdf:resource="#do_Youtube"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
  </swb:Class>
