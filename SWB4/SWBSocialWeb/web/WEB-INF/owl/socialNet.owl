      <swbxf:DisplayProperty rdf:ID="dp_hasYoutubeCategory">
        <swb:index rdf:datatype="http://www.w3.org/2001/XMLSchema#int"
        >200</swb:index>
        <swbxf:formElement rdf:resource="http://www.semanticwebbuilder.org/swb4/xforms/ontology#SelectMultipleGlobalWithBlank"/>
        <swbxf:propGroup rdf:resource="#pg_socialConfig"/>
      </swbxf:DisplayProperty>
    </swb:displayProperty>
