    <rdfs:subClassOf rdf:resource="#Oauthable"/>
  </owl:Class>
  <owl:ObjectProperty rdf:ID="postListenerBase">
    <owl:inverseOf>
      <owl:ObjectProperty rdf:ID="hasNewCreatedPostInv"/>
