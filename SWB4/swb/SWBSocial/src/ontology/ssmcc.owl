<?xml version="1.0"?>
<rdf:RDF
    xmlns:swbxf="http://www.semanticwebbuilder.org/swb4/xforms/ontology#"
    xmlns:smcc="http://www.owl-ontologies.com/ssmcc#"
    xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
    xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
    xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
    xmlns:owl="http://www.w3.org/2002/07/owl#"
    xmlns:swb="http://www.semanticwebbuilder.org/swb4/ontology#"
  xml:base="http://www.owl-ontologies.com/ssmcc">
  <owl:Ontology rdf:about="">
    <owl:imports rdf:resource="http://www.semanticwebbuilder.org/swb4/ontology"/>
    <owl:versionInfo rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >Created with TopBraid Composer</owl:versionInfo>
  </owl:Ontology>
  <owl:Class rdf:ID="Area">
    <rdfs:subClassOf rdf:resource="http://www.semanticwebbuilder.org/swb4/ontology#SWBClass"/>
    <rdfs:comment>Objeto Catálogo de áreas de una organización y a las cuales se dirijiran los mensajes (post)</rdfs:comment>
    <rdfs:subClassOf rdf:resource="http://www.semanticwebbuilder.org/swb4/ontology#Descriptiveable"/>
    <rdfs:subClassOf rdf:resource="http://www.semanticwebbuilder.org/swb4/ontology#Traceable"/>
    <rdf:type rdf:resource="http://www.semanticwebbuilder.org/swb4/ontology#Class"/>
  </owl:Class>
  <owl:Class rdf:ID="SocialUser">
    <rdfs:comment>Usuario que ha escrito un post en una red social</rdfs:comment>
    <rdfs:subClassOf rdf:resource="http://www.semanticwebbuilder.org/swb4/ontology#SWBClass"/>
    <rdfs:subClassOf rdf:resource="http://www.semanticwebbuilder.org/swb4/ontology#Traceable"/>
    <rdf:type rdf:resource="http://www.semanticwebbuilder.org/swb4/ontology#Class"/>
  </owl:Class>
  <owl:Class rdf:ID="SocialNetwork">
    <rdfs:comment>Objeto que se encarga de almacenar todos los datos de una red social</rdfs:comment>
    <rdfs:subClassOf rdf:resource="http://www.semanticwebbuilder.org/swb4/ontology#SWBClass"/>
    <rdfs:subClassOf rdf:resource="http://www.semanticwebbuilder.org/swb4/ontology#Descriptiveable"/>
    <rdfs:subClassOf rdf:resource="http://www.semanticwebbuilder.org/swb4/ontology#Traceable"/>
    <rdf:type rdf:resource="http://www.semanticwebbuilder.org/swb4/ontology#Class"/>
  </owl:Class>
  <owl:ObjectProperty rdf:ID="postToUserInv"/>
  <owl:ObjectProperty rdf:ID="postToUser">
    <rdfs:comment>Usuario al que va dirigido el mensaje (post)</rdfs:comment>
    <rdfs:label xml:lang="en">To User</rdfs:label>
    <rdfs:label xml:lang="es">Mensaje Dirigido a Usuario</rdfs:label>
    <owl:inverseOf rdf:resource="#postToUserInv"/>
    <rdfs:range rdf:resource="#SocialUser"/>
    <rdfs:domain>
      <swb:Class rdf:ID="Post">
        <rdfs:comment>Clase Post, esta es la principal, es donde se guarda cada post que trae el listener</rdfs:comment>
        <rdfs:subClassOf rdf:resource="http://www.semanticwebbuilder.org/swb4/ontology#SWBClass"/>
        <rdfs:subClassOf rdf:resource="http://www.semanticwebbuilder.org/swb4/ontology#Descriptiveable"/>
        <rdfs:subClassOf rdf:resource="http://www.semanticwebbuilder.org/swb4/ontology#Traceable"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
      </swb:Class>
    </rdfs:domain>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:ID="userSocialNetwork">
    <rdfs:domain rdf:resource="#SocialUser"/>
    <rdfs:range rdf:resource="#SocialNetwork"/>
    <swb:displayProperty>
      <swbxf:DisplayProperty rdf:ID="dp_userSocialNetwork">
        <swbxf:formElement rdf:resource="http://www.semanticwebbuilder.org/swb4/xforms/ontology#selectOneModelWithBlank"/>
      </swbxf:DisplayProperty>
    </swb:displayProperty>
    <swb:removeDependency rdf:datatype="http://www.w3.org/2001/XMLSchema#boolean"
    >true</swb:removeDependency>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:ID="userPostInv"/>
  <owl:ObjectProperty rdf:ID="postAreaInv">
    <rdfs:comment>Mensajes correspondientes al área en cuestión</rdfs:comment>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:ID="postArea">
    <owl:inverseOf rdf:resource="#postAreaInv"/>
    <rdfs:range rdf:resource="#Area"/>
    <rdfs:label>Area</rdfs:label>
    <rdfs:comment>Área de la Institución a la le corresponde el mensaje</rdfs:comment>
    <rdfs:domain rdf:resource="#Post"/>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:ID="postFromUser">
    <rdfs:label xml:lang="en">From User</rdfs:label>
    <rdfs:label xml:lang="es">Usuario Creador del Mansaje</rdfs:label>
    <rdfs:comment>Usuario del cual proviene el mensaje (post)</rdfs:comment>
    <swb:removeDependency rdf:datatype="http://www.w3.org/2001/XMLSchema#boolean"
    >true</swb:removeDependency>
    <rdfs:range rdf:resource="#SocialUser"/>
    <rdfs:domain rdf:resource="#Post"/>
    <owl:inverseOf rdf:resource="#userPostInv"/>
  </owl:ObjectProperty>
  <owl:DatatypeProperty rdf:ID="sn_url">
    <swb:displayProperty>
      <swbxf:DisplayProperty rdf:ID="dp_sn_url">
        <swb:index rdf:datatype="http://www.w3.org/2001/XMLSchema#int"
        >110</swb:index>
        <swbxf:formElement rdf:resource="http://www.semanticwebbuilder.org/swb4/xforms/ontology#TextBox"/>
      </swbxf:DisplayProperty>
    </swb:displayProperty>
    <rdfs:label>Url</rdfs:label>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    <rdfs:comment xml:lang="es">Url de conexión a la red social</rdfs:comment>
    <rdfs:domain rdf:resource="#SocialNetwork"/>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:ID="userId">
    <rdfs:domain rdf:resource="#SocialUser"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:ID="postLang">
    <rdfs:comment>Lenguaje en el que se encuentra el mensaje o post</rdfs:comment>
    <rdfs:label xml:lang="en">Languaje</rdfs:label>
    <rdfs:label xml:lang="es">Lenguaje</rdfs:label>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    <rdfs:domain rdf:resource="#Post"/>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:ID="sn_class">
    <swb:displayProperty>
      <swbxf:DisplayProperty rdf:ID="dp_sn_class">
        <swb:index rdf:datatype="http://www.w3.org/2001/XMLSchema#int"
        >120</swb:index>
        <swbxf:formElement rdf:resource="http://www.semanticwebbuilder.org/swb4/xforms/ontology#TextBox"/>
      </swbxf:DisplayProperty>
    </swb:displayProperty>
    <rdfs:domain rdf:resource="#SocialNetwork"/>
    <rdfs:label xml:lang="es">Nombre de Clase</rdfs:label>
    <rdfs:label xml:lang="en">Class Name</rdfs:label>
    <rdfs:comment>Fully Qualified Name de la Clase asociada para el manejo o implementación de una determinada red social (ej. org.semanticwb.social.networks.SWBSTwitterImp)</rdfs:comment>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:ID="postLatitude">
    <rdfs:domain rdf:resource="#Post"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    <rdfs:label xml:lang="es">Latitud</rdfs:label>
    <rdfs:label xml:lang="en">Latitude</rdfs:label>
    <rdfs:comment>Latitud desde donde se producjo el mensaje, esta la podemos representar en google maps</rdfs:comment>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:ID="postLongitude">
    <rdfs:comment>Longitud desde donde se produjo el mensaje, esta la podemos representar en google maps</rdfs:comment>
    <rdfs:label xml:lang="en">Longitude</rdfs:label>
    <rdfs:label xml:lang="es">Longitud</rdfs:label>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    <rdfs:domain rdf:resource="#Post"/>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:ID="sn_login">
    <swb:displayProperty>
      <swbxf:DisplayProperty rdf:ID="dp_sn_login">
        <swb:index rdf:datatype="http://www.w3.org/2001/XMLSchema#int"
        >130</swb:index>
        <swbxf:formElement rdf:resource="http://www.semanticwebbuilder.org/swb4/xforms/ontology#TextBox"/>
      </swbxf:DisplayProperty>
    </swb:displayProperty>
    <rdfs:comment>Login de conexión a la red social</rdfs:comment>
    <rdfs:label>login</rdfs:label>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    <rdfs:domain rdf:resource="#SocialNetwork"/>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:ID="postProfileImg">
    <rdfs:comment>Imagen de perfil en la red social</rdfs:comment>
    <rdfs:label xml:lang="en">Profile Image</rdfs:label>
    <rdfs:label xml:lang="es">Imagen de Perfil</rdfs:label>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    <rdfs:domain rdf:resource="#Post"/>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:ID="postMsg">
    <rdfs:domain rdf:resource="#Post"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    <rdfs:label xml:lang="es">Mensaje</rdfs:label>
    <rdfs:label xml:lang="en">Message</rdfs:label>
    <swb:displayProperty>
      <swbxf:DisplayProperty rdf:ID="dp_postMsg">
        <swbxf:formElement rdf:resource="http://www.semanticwebbuilder.org/swb4/xforms/ontology#TextArea_Description"/>
      </swbxf:DisplayProperty>
    </swb:displayProperty>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:ID="postSourceDevice">
    <rdfs:comment>Fuente desde la que se produce el mensaje (Twiitter para Iphone, Twiitter para Android, Web, etc)</rdfs:comment>
    <rdfs:label>Fuente desde la que se produce el mensaje</rdfs:label>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    <rdfs:domain rdf:resource="#Post"/>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:ID="postContent">
    <rdfs:comment>Publica un contentido</rdfs:comment>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:ID="sn_password">
    <swb:displayProperty>
      <swbxf:DisplayProperty rdf:ID="dp_sn_password">
        <swb:index rdf:datatype="http://www.w3.org/2001/XMLSchema#int"
        >140</swb:index>
        <swbxf:formElement rdf:resource="http://www.semanticwebbuilder.org/swb4/xforms/ontology#TextBox"/>
      </swbxf:DisplayProperty>
    </swb:displayProperty>
    <rdfs:comment>Password de conexión a la Red Social</rdfs:comment>
    <rdfs:label>Password</rdfs:label>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    <rdfs:domain rdf:resource="#SocialNetwork"/>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:ID="sn_name">
    <swb:displayProperty>
      <swbxf:DisplayProperty rdf:ID="dp_sn_name">
        <swb:index rdf:datatype="http://www.w3.org/2001/XMLSchema#int"
        >100</swb:index>
        <swbxf:formElement rdf:resource="http://www.semanticwebbuilder.org/swb4/xforms/ontology#TextBox"/>
      </swbxf:DisplayProperty>
    </swb:displayProperty>
    <rdfs:domain rdf:resource="#SocialNetwork"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
  </owl:DatatypeProperty>
  <swb:Class rdf:ID="Campaign">
    <rdfs:comment>Objeto que se encarga del catálogo de campañas</rdfs:comment>
    <rdfs:subClassOf rdf:resource="http://www.semanticwebbuilder.org/swb4/ontology#Traceable"/>
    <rdfs:subClassOf rdf:resource="http://www.semanticwebbuilder.org/swb4/ontology#Descriptiveable"/>
    <rdfs:label xml:lang="en">Campaign</rdfs:label>
    <rdfs:label xml:lang="es">Campaña</rdfs:label>
    <rdfs:subClassOf rdf:resource="http://www.semanticwebbuilder.org/swb4/ontology#SWBClass"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
  </swb:Class>
  <swb:CodePackage rdf:ID="pkg">
    <swb:prefix rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >smcc</swb:prefix>
    <swb:package rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >org.swb.social.model</swb:package>
  </swb:CodePackage>
</rdf:RDF>

