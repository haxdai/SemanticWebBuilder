            <input type="file" name="template" label="Modelo (xsl, xslt)" filetype="xsl|xslt" jsrequired="false" showfile="true" isshowfiletype="true" isremovefile="true" showmsg="Mostrar modelo..." editmsg="Editar modelo..." removemsg="Remover modelo" msg="Modelo atual: " bydefault="{@webpath}/swbadmin/xsl/Language/Language.xslt" msgbydefault="Modelo padrão: " />
